body {
  /*font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;*/
  font-family: "微软雅黑", "Helvetica Neue", Helvetica, "PingFang SC", Arial, sans-serif;
  font-size: 14px;
  line-height: 26px;
  color: #333;
  background-color: #fff;
}
body,
html {
  min-width: 1200px;
  overflow-x: auto;
}
/*表单默认属性*/
input::-webkit-input-placeholder {
  color: #cccccc;
  opacity: 1;
}
textarea::-webkit-input-placeholder {
  color: #cccccc;
  opacity: 1;
}
input,
textarea {
  border-color: #CCCCCC;
  font-size: 14px;
  line-height: 26px;
  resize: none;
  outline: none;
  border: 1px solid #CCCCCC;
  text-indent: 10px;
  box-sizing: border-box;
}
textarea:focus {
  border-color: #D6A003;
}
input:focus {
  border-color: #D6A003;
}
.mg0 {
  margin: 0!important;
}
.mr10 {
  margin-right: 10px;
}
h3 {
  font-weight: normal;
}
.bdno {
  border: none!important;
}
.clearfloat:after {
  display: block;
  content: '';
  height: 0;
  width: 0;
  clear: both;
}
.container {
  width: 1200px;
  padding-right: 10px;
  padding-left: 10px;
  margin-right: auto;
  margin-left: auto;
  position: relative;
}

/*内容*/
.main {
  background: #fff;
}
.main .main-box {
  background: url("../../../images/personal-homepage/bg.png") no-repeat;
  background-size: 100% auto;
  padding-top: 38px;
}
.main .main-box .grid-1 {
  background: #303030;
  padding: 45px 75px 25px 45px;
  position: relative;
}
.main .main-box .grid-1 .head-img {
  position: relative;
  width: 115px;
  height: 115px;
  float: left;
}
.main .main-box .grid-1 .head-img img {
  height: 100%;
  width: 100%;
  display: block;
  border-radius: 50%;
}
.main .main-box .grid-1 .head-img i.v {
  width: 23px;
  height: 23px;
  display: block;
  position: absolute;
  background: url("../../../images/personal-homepage/shiming.png") no-repeat;
  background-size: 100% 100%;
  right: 12px;
  bottom: 0;
}
.main .main-box .grid-1 .personal-info {
  padding: 25px 20px;
  float: left;
}
.main .main-box .grid-1 .personal-info .user-name {
  font-size: 20px;
  color: #C29F4A;
  margin-bottom: 20px;
}
.main .main-box .grid-1 .personal-info .user-name span {
  width: 67px;
  height: 25px;
  background: url("../../../images/personal-homepage/pthuiyuan.png") no-repeat;
  background-size: 100% 100%;
  border-radius: 20px;
  display: inline-block;
  margin-right: 10px;
  position: relative;
  top: 5px;
}
.main .main-box .grid-1 .personal-info .user-name .sex {
  width: 16px;
  height: 19px;
  display: inline-block;
  background: url("../../../images/personal-homepage/women.png") no-repeat;
  background-size: 100% 100%;
  margin-left: 20px;
  position: relative;
  top: 3px;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit {
  display: block;
  float: left;
  color: #CCCCCC;
  margin-right: 30px;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit i {
  position: relative;
  top: 2px;
  margin-right: 5px;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit .address {
  display: inline-block;
  width: 13px;
  height: 17px;
  background: url("../../../images/personal-homepage/address.png") no-repeat;
  background-size: 100% 100%;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit .star {
  display: inline-block;
  width: 18px;
  height: 18px;
  background: url("../../../images/personal-homepage/star.png") no-repeat;
  background-size: 100% 100%;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit .hot {
  display: inline-block;
  width: 13px;
  height: 17px;
  background: url("../../../images/personal-homepage/hot.png") no-repeat;
  background-size: 100% 100%;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit .gift {
  display: inline-block;
  width: 17px;
  height: 15px;
  background: url("../../../images/personal-homepage/gift.png") no-repeat;
  background-size: 100% 100%;
}
.main .main-box .grid-1 .sign-in-btn {
  width: 154px;
  height: 50px;
  line-height: 50px;
  color: #fff;
  border: 2px solid #fff;
  border-radius: 30px;
  text-align: center;
  cursor: pointer;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 75px;
  margin: auto;
}
.main .main-box .grid-1 .sign-in-btn .sign-in {
  width: 24px;
  height: 24px;
  display: inline-block;
  position: relative;
  top: 5px;
  background: url("../../../images/personal-homepage/sign-in.png") no-repeat;
  background-size: 100% 100%;
  margin-right: 12px;
}
.main .main-box .grid-2 {
  padding: 30px 0;
}
.main .main-box .grid-2 .tabs1 .hd1 {
  width: 208px;
  float: left;
  background: #fff;
  margin-right: 10px;
}
.main .main-box .grid-2 .tabs1 .hd1 ul {
  border: 1px solid #E6E6E6;
  border-bottom: 0;
}
.main .main-box .grid-2 .tabs1 .hd1 ul li {
  line-height: 66px;
  font-size: 16px;
  border-bottom: 1px solid #E6E6E6;
  position: relative;
  cursor: pointer;
}
.main .main-box .grid-2 .tabs1 .hd1 ul li a {
  padding-left: 49px;
  display: block;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.main .main-box .grid-2 .tabs1 .hd1 ul .on {
  background: #000;
}
.main .main-box .grid-2 .tabs1 .hd1 ul .on a {
  color: #CBAD50;
}
.main .main-box .grid-2 .tabs1 .hd1 ul .on .right {
  width: 10px;
  height: 15px;
  background: url("../../../images/about-us/right.png") no-repeat;
  background-size: 100% 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 25px;
  margin: auto;
}
.main .main-box .grid-2 .tabs1 .bd1 {
  width: 975px;
  float: right;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 {
  border: 1px solid #E6E6E6;
  background: #fff;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title {
  height: 88px;
  padding-left: 45px;
  padding-right: 28px;
  border-bottom: 1px solid #E6E6E6;
  position: relative;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .title {
  float: left;
  font-size: 18px;
  line-height: 88px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .title span {
  color: #DBA02C;
  font-size: 14px;
  margin-left: 18px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .title span img {
  display: inline-block;
  margin-right: 8px;
  position: relative;
  top: 4px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content {
  padding-top: 20px;
  margin-bottom: 66px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list {
  padding: 7px 0;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-title {
  width: 108px;
  float: left;
  line-height: 42px;
  font-size: 16px;
  text-align: right;
  margin-right: 25px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-input {
  float: left;
  display: block;
  line-height: 42px;
  width: 520px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-label {
  float: left;
  display: block;
  width: 782px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-label span,
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-label em {
  display: block;
  border: 1px solid #CCC;
  width: 110px;
  line-height: 48px;
  text-align: center;
  font-size: 16px;
  border-radius: 3px;
  float: left;
  margin-right: 17px;
  margin-bottom: 17px;
  cursor: pointer;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-label .add-label {
  border: 1px dashed #CCC;
  position: relative;
  padding-left: 30px;
  box-sizing: border-box;
  width: 112px;
  color: #ccc;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-label .add-label i {
  font-size: 35px;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 17px;
  margin: auto;
  line-height: 40px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-label span:hover {
  border-color: #DBA02C;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-label span.active {
  background: #DBA02C;
  color: #fff;
  border-color: #DBA02C;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-label p {
  float: left;
  width: 100%;
  color: #DBA02C;
  font-size: 14px;
  position: relative;
  top: -10px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list textarea {
  display: block;
  float: left;
  width: 520px;
  height: 160px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box {
  float: left;
  width: 520px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box .select {
  position: relative;
  float: left;
  margin-bottom: 15px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box .select input {
  width: 255px;
  height: 42px;
  padding-right: 30px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box .select .arrow {
  position: absolute;
  right: 16px;
  top: 0;
  bottom: 0;
  margin: auto;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box5 {
  width: 785px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box5 .select {
  margin-bottom: 0;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box5 .subhead {
  line-height: 42px;
  color: #DBA02C;
  font-size: 14px;
  margin-left: 10px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box6 {
  width: 785px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box6 .select {
  margin-bottom: 0;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box6 .subhead {
  line-height: 42px;
  color: #000;
  font-size: 14px;
  margin-left: 10px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .subhead11 {
  padding-top: 7px;
  padding-left: 133px;
  float: left;
  display: block;
  width: 100%;
  color: #DBA02C;
  font-size: 14px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .upload-pictures {
  width: 202px;
  height: 150px;
  float: left;
  border: 1px dashed #ccc;
  position: relative;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .upload-pictures span {
  width: 60px;
  height: 18px;
  line-height: 18px;
  padding-left: 30px;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  color: #DBA02C;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .upload-pictures span i {
  font-size: 50px;
  position: absolute;
  top: -4px;
  left: -8px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box12 {
  padding-top: 30px;
  padding-left: 133px;
  float: left;
  display: block;
  width: 100%;
  margin-bottom: 30px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box12 .photo {
  width: 204px;
  height: 152px;
  float: left;
  margin-right: 20px;
  margin-bottom: 20px;
  position: relative;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box12 .photo img {
  display: block;
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box12 .photo .f-photo {
  height: 35px;
  padding: 9px 0;
  position: absolute;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  width: 100%;
  box-sizing: border-box;
  opacity: 0;
  filter: alpha(opacity=0);
  cursor: pointer;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box12 .photo .f-photo span {
  display: block;
  float: left;
  width: 50%;
  box-sizing: border-box;
  line-height: 17px;
  color: #fff;
  text-align: center;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box12 .photo .f-photo span:nth-of-type(1) {
  border-right: 1px solid #737378;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .list-box12 .photo:hover .f-photo {
  opacity: 1;
  filter: alpha(opacity=100);
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .submit {
  float: left;
  width: 216px;
  height: 50px;
  background: #D6A003;
  color: #fff;
  line-height: 50px;
  text-align: center;
  cursor: pointer;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .list .submit:active {
  background: #B38608;
}
/*底部样式*/
.footer {
  background: #18292c;
}
.footer .content {
  padding: 36px 40px 0;
}
.footer .content .nav li {
  float: left;
  margin-right: 160px;
}
.footer .content .nav li p {
  font-size: 24px;
  line-height: 24px;
  color: #8c9496;
  margin-bottom: 18px;
}
.footer .content .nav li a {
  font-size: 16px;
  line-height: 30px;
  color: #48666a;
  padding: 5px 0;
  display: block;
}
.footer .content .code {
  border: 1px solid #465456;
  padding: 10px;
}
.footer .content .code img {
  margin-bottom: 8px;
}
.footer .content .code p {
  font-size: 14px;
  line-height: 14px;
  color: #48666a;
  text-align: center;
  margin-top: 4px;
}
.footer .copyright {
  padding: 40px 0;
  font-size: 12px;
  line-height: 20px;
  color: #10434f;
  text-align: center;
}
.fr {
  float: right!important;
}
.fl {
  float: left!important;
}
