body {
  /*font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;*/
  font-family: "PingFang SC", "微软雅黑", "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 14px;
  line-height: 26px;
  color: #333;
  background-color: #fff;
}
body,
html {
  min-width: 1200px;
  overflow-x: auto;
}
/*表单默认属性*/
input::-webkit-input-placeholder {
  color: #cccccc;
  opacity: 1;
}
textarea::-webkit-input-placeholder {
  color: #cccccc;
  opacity: 1;
}
input,
textarea {
  border-color: #CCCCCC;
  font-size: 14px;
  line-height: 26px;
  resize: none;
  outline: none;
  border: 1px solid #CCCCCC;
  text-indent: 10px;
  box-sizing: border-box;
}
textarea:focus {
  border-color: #D6A003;
}
input:focus {
  border-color: #D6A003;
}
.mg0 {
  margin: 0!important;
}
h3 {
  font-weight: normal;
}
.bdno {
  border: none!important;
}
.silver {
  background: #FAFAFA;
}
.clearfloat:after {
  display: block;
  content: '';
  height: 0;
  width: 0;
  clear: both;
}
.container {
  width: 1200px;
  padding-right: 10px;
  padding-left: 10px;
  margin-right: auto;
  margin-left: auto;
  position: relative;
}

/*内容*/
.main {
  background: #fff;
}
.main .main-box {
  background: url("../../../images/personal-homepage/bg.png") no-repeat;
  background-size: 100% auto;
  padding-top: 38px;
}
.main .main-box .grid-1 {
  background: #303030;
  padding: 45px 75px 25px 45px;
  position: relative;
}
.main .main-box .grid-1 .head-img {
  position: relative;
  width: 115px;
  height: 115px;
  float: left;
}
.main .main-box .grid-1 .head-img img {
  height: 100%;
  width: 100%;
  display: block;
  border-radius: 50%;
}
.main .main-box .grid-1 .head-img i.v {
  width: 23px;
  height: 23px;
  display: block;
  position: absolute;
  background: url("../../../images/personal-homepage/shiming.png") no-repeat;
  background-size: 100% 100%;
  right: 12px;
  bottom: 0;
}
.main .main-box .grid-1 .personal-info {
  padding: 25px 20px;
  float: left;
}
.main .main-box .grid-1 .personal-info .user-name {
  font-size: 20px;
  color: #C29F4A;
  margin-bottom: 20px;
}
.main .main-box .grid-1 .personal-info .user-name span {
  width: 67px;
  height: 25px;
  background: url("../../../images/personal-homepage/pthuiyuan.png") no-repeat;
  background-size: 100% 100%;
  border-radius: 20px;
  display: inline-block;
  margin-right: 10px;
  position: relative;
  top: 5px;
}
.main .main-box .grid-1 .personal-info .user-name .sex {
  width: 16px;
  height: 19px;
  display: inline-block;
  background: url("../../../images/personal-homepage/women.png") no-repeat;
  background-size: 100% 100%;
  margin-left: 20px;
  position: relative;
  top: 3px;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit {
  display: block;
  float: left;
  color: #CCCCCC;
  margin-right: 30px;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit i {
  position: relative;
  top: 2px;
  margin-right: 5px;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit .address {
  display: inline-block;
  width: 13px;
  height: 17px;
  background: url("../../../images/personal-homepage/address.png") no-repeat;
  background-size: 100% 100%;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit .star {
  display: inline-block;
  width: 18px;
  height: 18px;
  background: url("../../../images/personal-homepage/star.png") no-repeat;
  background-size: 100% 100%;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit .hot {
  display: inline-block;
  width: 13px;
  height: 17px;
  background: url("../../../images/personal-homepage/hot.png") no-repeat;
  background-size: 100% 100%;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit .gift {
  display: inline-block;
  width: 17px;
  height: 15px;
  background: url("../../../images/personal-homepage/gift.png") no-repeat;
  background-size: 100% 100%;
}
.main .main-box .grid-1 .sign-in-btn {
  width: 154px;
  height: 50px;
  line-height: 50px;
  color: #fff;
  border: 2px solid #fff;
  border-radius: 30px;
  text-align: center;
  cursor: pointer;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 75px;
  margin: auto;
}
.main .main-box .grid-1 .sign-in-btn .sign-in {
  width: 24px;
  height: 24px;
  display: inline-block;
  position: relative;
  top: 5px;
  background: url("../../../images/personal-homepage/sign-in.png") no-repeat;
  background-size: 100% 100%;
  margin-right: 12px;
}
.main .main-box .grid-2 {
  padding: 30px 0;
}
.main .main-box .grid-2 .tabs1 .hd1 {
  width: 208px;
  float: left;
  background: #fff;
  margin-right: 10px;
}
.main .main-box .grid-2 .tabs1 .hd1 ul {
  border: 1px solid #E6E6E6;
  border-bottom: 0;
}
.main .main-box .grid-2 .tabs1 .hd1 ul li {
  line-height: 66px;
  font-size: 16px;
  border-bottom: 1px solid #E6E6E6;
  position: relative;
  cursor: pointer;
}
.main .main-box .grid-2 .tabs1 .hd1 ul li a {
  padding-left: 49px;
  display: block;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.main .main-box .grid-2 .tabs1 .hd1 ul .on {
  background: #000;
}
.main .main-box .grid-2 .tabs1 .hd1 ul .on a {
  color: #CBAD50;
}
.main .main-box .grid-2 .tabs1 .hd1 ul .on .right {
  width: 10px;
  height: 15px;
  background: url("../../../images/about-us/right.png") no-repeat;
  background-size: 100% 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 25px;
  margin: auto;
}
.main .main-box .grid-2 .tabs1 .bd1 {
  width: 975px;
  float: right;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 {
  border: 1px solid #E6E6E6;
  background: #fff;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title {
  padding-top: 50px;
  border-bottom: 1px solid #E6E6E6;
  padding-left: 30px;
  background: #FAFAFA;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-1 {
  font-size: 30px;
  margin-bottom: 20px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-1 i {
  color: #F8421D;
  margin-right: 125px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-1 span {
  display: inline-block;
  line-height: 28px;
  height: 28px;
  border: 2px solid #D2A929;
  text-align: center;
  font-size: 14px;
  color: #D2A929;
  border-radius: 3px;
  padding: 0 20px;
  margin-right: 28px;
  position: relative;
  top: -5px;
  cursor: pointer;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-1 span:active {
  border-color: #B38608;
  color: #B38608;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-2 {
  font-size: 18px;
  margin-bottom: 40px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-2 i {
  color: #F8421D;
  margin-right: 12px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-2 img {
  display: inline-block;
  position: relative;
  top: 4px;
  margin-right: 5px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-2 span {
  color: #999;
  font-size: 14px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-1 {
  padding-top: 45px;
  padding-bottom: 32px;
  border-bottom: 1px solid #E6E6E6;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-1 .u-1 {
  float: left;
  box-sizing: border-box;
  width: 50%;
  text-align: center;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-1 .u-1 p.money {
  font-size: 18px;
  color: #F85C3C;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-1 .u-1 p {
  color: #000;
  font-size: 14px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-1 .u-1:nth-of-type(1) {
  border-right: 1px solid #E6E6E6;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 {
  padding-left: 33px;
  padding-right: 65px;
  padding-bottom: 55px;
  border-bottom: 1px solid #E6E6E6;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-title {
  padding: 33px 0;
  font-size: 18px;
  color: #DCA331;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-radio {
  margin-bottom: 25px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-radio .checked-box {
  height: 30px;
  width: 20px;
  float: left;
  position: relative;
  margin-right: 15px;
  cursor: pointer;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-radio .checked-box .checked {
  width: 20px;
  height: 20px;
  display: block;
  border: 1px solid #B7B7B7;
  border-radius: 50%;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-radio .checked-box .checked .c {
  display: block;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-radio .info {
  float: left;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-radio .info .info-title {
  width: 150px;
  height: 30px;
  color: #fff;
  font-size: 16px;
  line-height: 30px;
  text-align: center;
  background: #3B5A5E;
  border-radius: 3px;
  margin-bottom: 25px;
  cursor: pointer;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-radio .info p {
  margin-bottom: 5px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-radio .info p span {
  color: #DCA331;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-number {
  margin-bottom: 55px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-number span {
  line-height: 26px;
  color: #696969;
  margin-right: 20px;
  display: block;
  float: left;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-number .input-number {
  float: left;
  width: 113px;
  height: 26px;
  box-sizing: border-box;
  border: 1px solid #999;
  margin-right: 8px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-number .input-number .add,
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-number .input-number input,
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-number .input-number .minus {
  float: left;
  display: block;
  height: 24px;
  line-height: 24px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-number .input-number .add,
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-number .input-number .minus {
  width: 25px;
  margin: 0;
  text-align: center;
  background: #E6E6E6;
  font-size: 22px;
  cursor: pointer;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-number .input-number input {
  width: 60px;
  border-top: 0;
  border-bottom: 0;
  border-left: 1px solid #999;
  border-right: 1px solid #999;
  text-align: center;
  padding-right: 10px;
  font-size: 12px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-btn {
  padding-left: 35px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-btn span {
  display: block;
  width: 215px;
  height: 50px;
  line-height: 50px;
  text-align: center;
  background: #D6A003;
  color: #fff;
  cursor: pointer;
  border-radius: 3px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .u-btn span:active {
  background: #B38608;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-2 .mod-2-content .active .checked-box .checked .c {
  background: url("../../../images/personal-homepage/pay-center/c.png") no-repeat;
  background-size: 75% 75%;
  background-position: 2px 2px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .mod-3-title {
  line-height: 90px;
  padding-left: 30px;
  font-size: 18px;
  border-bottom: 1px solid #E6E6E6;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .mod-3-content {
  margin-bottom: 55px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .mod-3-content .list {
  border-bottom: 1px solid #E6E6E6;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .mod-3-content .list ul li {
  line-height: 68px;
  text-align: center;
  float: left;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .mod-3-content .list ul .time {
  width: 167px;
  border-right: 1px solid #E6E6E6;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .mod-3-content .list ul .content {
  width: 311px;
  border-right: 1px solid #E6E6E6;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .mod-3-content .list ul .state {
  width: 137px;
  border-right: 1px solid #E6E6E6;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .mod-3-content .list ul .number {
  width: 137px;
  border-right: 1px solid #E6E6E6;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .mod-3-content .list ul .surplus {
  width: 217px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .page-btn {
  width: 345px;
  margin: 0 auto;
  padding-bottom: 55px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .page-btn i,
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .page-btn span {
  display: block;
  float: left;
  text-align: center;
  line-height: 33px;
  border: 1px solid #ccc;
  cursor: pointer;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .page-btn i {
  width: 53px;
  margin-right: 12px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .page-btn span {
  width: 33px;
  margin-right: 7px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .mod-3 .page-btn .active {
  background: #000;
  border-color: #000;
  color: #fff;
}
/*底部样式*/
.footer {
  background: #18292c;
}
.footer .content {
  padding: 36px 40px 0;
}
.footer .content .nav li {
  float: left;
  margin-right: 160px;
}
.footer .content .nav li p {
  font-size: 24px;
  line-height: 24px;
  color: #8c9496;
  margin-bottom: 18px;
}
.footer .content .nav li a {
  font-size: 16px;
  line-height: 30px;
  color: #48666a;
  padding: 5px 0;
  display: block;
}
.footer .content .code {
  border: 1px solid #465456;
  padding: 10px;
}
.footer .content .code img {
  margin-bottom: 8px;
}
.footer .content .code p {
  font-size: 14px;
  line-height: 14px;
  color: #48666a;
  text-align: center;
  margin-top: 4px;
}
.footer .copyright {
  padding: 40px 0;
  font-size: 12px;
  line-height: 20px;
  color: #10434f;
  text-align: center;
}
.fr {
  float: right!important;
}
.fl {
  float: left!important;
}
