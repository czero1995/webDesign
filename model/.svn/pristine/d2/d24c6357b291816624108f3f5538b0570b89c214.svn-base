/*内容*/
.banner {
  height: 500px;
  width: 100%;
  /*顶部轮播图样式*/
  /*轮播图样式结束*/
}
.banner .pc-slide {
  width: 100%;
  margin: 0 auto;
  position: relative;
}
.banner .swiper-slide a img {
  width: 100%;
  height: 100%;
  display: block;
}
.banner .view .swiper-container {
  width: 100%;
  height: 500px;
}
.banner .view .arrow-left {
  background: url(images/index_tab_l.png) no-repeat left top;
  position: absolute;
  left: 10px;
  top: 50%;
  margin-top: -25px;
  width: 28px;
  height: 51px;
  z-index: 10;
}
.banner .view .arrow-right {
  background: url(images/index_tab_r.png) no-repeat left bottom;
  position: absolute;
  right: 10px;
  top: 50%;
  margin-top: -25px;
  width: 28px;
  height: 51px;
  z-index: 10;
}
.banner .preview {
  width: 100%;
  margin-top: 10px;
  position: absolute;
  bottom: 0;
  background: #000000;
  padding: 15px 0;
}
.banner .preview .swiper-container {
  width: 1200px;
  height: 133px;
}
.banner .preview .swiper-slide {
  width: 219px;
  height: 133px;
}
.banner .preview .arrow-left {
  background: url(images/feel3.png) no-repeat left top;
  position: absolute;
  left: 10px;
  top: 50%;
  margin-top: -9px;
  width: 9px;
  height: 18px;
  z-index: 10;
}
.banner .preview .arrow-right {
  background: url(images/feel4.png) no-repeat left bottom;
  position: absolute;
  right: 10px;
  top: 50%;
  margin-top: -9px;
  width: 9px;
  height: 18px;
  z-index: 10;
}
.banner .preview img {
  padding: 1px;
  width: 100%;
  height: 100%;
  display: block;
}
.banner .preview .active-nav img {
  padding: 0;
}
