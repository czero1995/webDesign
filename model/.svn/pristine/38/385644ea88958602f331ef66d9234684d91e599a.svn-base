body {
  /*font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;*/
  font-family: "PingFang SC", "微软雅黑", "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 14px;
  line-height: 26px;
  color: #333;
  background-color: #fff;
}
body,
html {
  min-width: 1200px;
  overflow-x: auto;
}
/*表单默认属性*/
input::-webkit-input-placeholder {
  color: #cccccc;
  opacity: 1;
}
textarea::-webkit-input-placeholder {
  color: #cccccc;
  opacity: 1;
}
input,
textarea {
  border-color: #CCCCCC;
  font-size: 14px;
  line-height: 26px;
  resize: none;
  outline: none;
  border: 1px solid #CCCCCC;
  text-indent: 10px;
}
textarea:focus {
  border-color: #D6A003;
}
input:focus {
  border-color: #D6A003;
}
.mg0 {
  margin: 0!important;
}
.mr40 {
  margin-right: 40px;
}
h3 {
  font-weight: normal;
}
.bdno {
  border: none!important;
}
.clearfloat:after {
  display: block;
  content: '';
  height: 0;
  width: 0;
  clear: both;
}
.container {
  width: 1200px;
  padding-right: 10px;
  padding-left: 10px;
  margin-right: auto;
  margin-left: auto;
  position: relative;
}

/*内容*/
.main {
  background: #fff;
}
.main .main-box {
  background: url("../../../images/personal-homepage/bg.png") no-repeat;
  background-size: 100% auto;
  padding-top: 38px;
}
.main .main-box .grid-1 {
  background: #303030;
  padding: 45px 75px 25px 45px;
  position: relative;
}
.main .main-box .grid-1 .head-img {
  position: relative;
  width: 115px;
  height: 115px;
  float: left;
}
.main .main-box .grid-1 .head-img img {
  height: 100%;
  width: 100%;
  display: block;
  border-radius: 50%;
}
.main .main-box .grid-1 .head-img i.v {
  width: 23px;
  height: 23px;
  display: block;
  position: absolute;
  background: url("../../../images/personal-homepage/shiming.png") no-repeat;
  background-size: 100% 100%;
  right: 12px;
  bottom: 0;
}
.main .main-box .grid-1 .personal-info {
  padding: 25px 20px;
  float: left;
}
.main .main-box .grid-1 .personal-info .user-name {
  font-size: 20px;
  color: #C29F4A;
  margin-bottom: 20px;
}
.main .main-box .grid-1 .personal-info .user-name span {
  width: 67px;
  height: 25px;
  background: url("../../../images/personal-homepage/pthuiyuan.png") no-repeat;
  background-size: 100% 100%;
  border-radius: 20px;
  display: inline-block;
  margin-right: 10px;
  position: relative;
  top: 5px;
}
.main .main-box .grid-1 .personal-info .user-name .sex {
  width: 16px;
  height: 19px;
  display: inline-block;
  background: url("../../../images/personal-homepage/women.png") no-repeat;
  background-size: 100% 100%;
  margin-left: 20px;
  position: relative;
  top: 3px;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit {
  display: block;
  float: left;
  color: #CCCCCC;
  margin-right: 30px;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit i {
  position: relative;
  top: 2px;
  margin-right: 5px;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit .address {
  display: inline-block;
  width: 13px;
  height: 17px;
  background: url("../../../images/personal-homepage/address.png") no-repeat;
  background-size: 100% 100%;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit .star {
  display: inline-block;
  width: 18px;
  height: 18px;
  background: url("../../../images/personal-homepage/star.png") no-repeat;
  background-size: 100% 100%;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit .hot {
  display: inline-block;
  width: 13px;
  height: 17px;
  background: url("../../../images/personal-homepage/hot.png") no-repeat;
  background-size: 100% 100%;
}
.main .main-box .grid-1 .personal-info .user-info .user-info-unit .gift {
  display: inline-block;
  width: 17px;
  height: 15px;
  background: url("../../../images/personal-homepage/gift.png") no-repeat;
  background-size: 100% 100%;
}
.main .main-box .grid-1 .sign-in-btn {
  width: 154px;
  height: 50px;
  line-height: 50px;
  color: #fff;
  border: 2px solid #fff;
  border-radius: 30px;
  text-align: center;
  cursor: pointer;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 75px;
  margin: auto;
}
.main .main-box .grid-1 .sign-in-btn .sign-in {
  width: 24px;
  height: 24px;
  display: inline-block;
  position: relative;
  top: 5px;
  background: url("../../../images/personal-homepage/sign-in.png") no-repeat;
  background-size: 100% 100%;
  margin-right: 12px;
}
.main .main-box .grid-2 {
  padding: 30px 0;
}
.main .main-box .grid-2 .tabs1 .hd1 {
  width: 208px;
  float: left;
  background: #fff;
  margin-right: 10px;
}
.main .main-box .grid-2 .tabs1 .hd1 ul {
  border: 1px solid #E6E6E6;
  border-bottom: 0;
}
.main .main-box .grid-2 .tabs1 .hd1 ul li {
  line-height: 66px;
  font-size: 16px;
  border-bottom: 1px solid #E6E6E6;
  position: relative;
  cursor: pointer;
}
.main .main-box .grid-2 .tabs1 .hd1 ul li a {
  padding-left: 49px;
  display: block;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.main .main-box .grid-2 .tabs1 .hd1 ul .on {
  background: #000;
}
.main .main-box .grid-2 .tabs1 .hd1 ul .on a {
  color: #CBAD50;
}
.main .main-box .grid-2 .tabs1 .hd1 ul .on .right {
  width: 10px;
  height: 15px;
  background: url("../../../images/about-us/right.png") no-repeat;
  background-size: 100% 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 25px;
  margin: auto;
}
.main .main-box .grid-2 .tabs1 .bd1 {
  width: 975px;
  float: right;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 {
  border: 1px solid #E6E6E6;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title {
  height: 147px;
  border-bottom: 1px solid #E6E6E6;
  padding-left: 74px;
  padding-top: 30px;
  background: #FAFAFA;
  box-sizing: border-box;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-plan:nth-of-type(1) .u-plan-content {
  margin-left: 0;
  padding-left: 0px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-plan {
  float: left;
  position: relative;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-plan .hr {
  float: left;
  border-bottom: 1px dashed #ccc;
  height: 0;
  width: 60px;
  position: absolute;
  top: 25px;
  left: 0;
  margin: auto;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-plan .u-plan-content {
  float: left;
  margin-right: 20px;
  padding-left: 80px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-plan .u-plan-content .u-plan-img {
  width: 52px;
  height: 52px;
  margin: 0 auto;
  margin-bottom: 5px;
  background-size: 100% 100%;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-plan .u-plan-content p {
  color: #A1A1A1;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-plan .u-plan-content .u-plan-1 {
  background: url("../../../images/personal-homepage/manage-annunciate/pic1-2.png");
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-plan .u-plan-content .u-plan-2 {
  background: url("../../../images/personal-homepage/manage-annunciate/pic2-2.png");
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-plan .u-plan-content .u-plan-3 {
  background: url("../../../images/personal-homepage/manage-annunciate/pic3-2.png");
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-plan .u-plan-content .u-plan-4 {
  background: url("../../../images/personal-homepage/manage-annunciate/pic4-2.png");
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-plan .u-plan-content .u-plan-5 {
  background: url("../../../images/personal-homepage/manage-annunciate/pic5-2.png");
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .u-plan .u-plan-content .u-plan-6 {
  background: url("../../../images/personal-homepage/manage-annunciate/pic6-2.png");
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .active .hr {
  border-bottom: 1px dashed #DFAB44;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .active .u-plan-content p {
  color: #DFAB44;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .active .u-plan-content .u-plan-1 {
  background: url("../../../images/personal-homepage/manage-annunciate/pic1-1.png");
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .active .u-plan-content .u-plan-2 {
  background: url("../../../images/personal-homepage/manage-annunciate/pic2-1.png");
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .active .u-plan-content .u-plan-3 {
  background: url("../../../images/personal-homepage/manage-annunciate/pic3-1.png");
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .active .u-plan-content .u-plan-4 {
  background: url("../../../images/personal-homepage/manage-annunciate/pic4-1.png");
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .active .u-plan-content .u-plan-5 {
  background: url("../../../images/personal-homepage/manage-annunciate/pic5-1.png");
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-title .active .u-plan-content .u-plan-6 {
  background: url("../../../images/personal-homepage/manage-annunciate/pic6-1.png");
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content {
  padding-top: 28px;
  background: #fff;
  padding-left: 29px;
  padding-right: 29px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .dropdown-box .dropdown {
  width: 253px;
  height: 40px;
  line-height: 40px;
  padding-left: 13px;
  border: 1px solid #E6E6E6;
  position: relative;
  margin-bottom: 23px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .dropdown-box .dropdown .caret {
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  right: 15px;
  display: block;
  width: 0;
  height: 0;
  vertical-align: middle;
  border-top: 6px dashed #999;
  border-right: 6px solid transparent;
  border-left: 6px solid transparent;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li {
  border: 1px solid #E6E6E6;
  padding: 13px;
  margin-bottom: 26px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-logo {
  width: 235px;
  height: 177px;
  float: left;
  margin-right: 30px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-logo img {
  width: 100%;
  height: 100%;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-info {
  padding-top: 15px;
  width: 460px;
  float: left;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-info p.title {
  font-size: 16px;
  color: #A7781E;
  margin-bottom: 5px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-info p.time {
  font-size: 12px;
  color: #8F8F8F;
  margin-bottom: 15px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-info p {
  font-size: 14px;
  color: #777;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-state {
  float: right;
  padding-top: 15px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-state .flag {
  display: inline-block;
  width: 14px;
  height: 19px;
  margin-right: 8px;
  background-size: 100% 100%;
  position: relative;
  top: 3px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-state .mod-1 {
  margin-bottom: 20px;
  padding-left: 40px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-state .mod-2 {
  width: 105px;
  line-height: 32px;
  text-align: center;
  border: 2px solid #D2A929;
  border-radius: 3px;
  font-size: 12px;
  color: #E4BA72;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-state .mod-3 {
  color: #DDA635;
  padding-left: 12px;
  padding-top: 15px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-state .mod-3 span {
  display: inline-block;
  padding-bottom: 3px;
  border-bottom: 1px solid #DDA635;
  position: relative;
  line-height: 15px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-state .mod-3 i.down {
  width: 15px;
  height: 16px;
  display: inline-block;
  background: url("../../../images/personal-homepage/manage-annunciate/down.png") no-repeat;
  background-size: 100% 100%;
  position: absolute;
  right: -20px;
  top: 0;
  bottom: 0;
  margin: auto;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-state .yellow {
  color: #F6BA41;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-state .yellow .flag {
  background: url("../../../images/personal-homepage/manage-annunciate/yellowflag.png") no-repeat;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-state .red {
  color: #EC404F;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-state .red .flag {
  background: url("../../../images/personal-homepage/manage-annunciate/redflag.png") no-repeat;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-state .silver {
  color: #7A7A7A;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list ul li .annunciate-state .silver .flag {
  background: url("../../../images/personal-homepage/manage-annunciate/silverflag.png") no-repeat;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list .page-btn {
  margin: 0 auto;
  padding-bottom: 60px;
  width: 345px;
  padding-top: 45px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list .page-btn i,
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list .page-btn span {
  display: inline-block;
  line-height: 33px;
  border: 1px solid #ccc;
  text-align: center;
  color: #737373;
  cursor: pointer;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list .page-btn i {
  width: 53px;
  margin-right: 12px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list .page-btn span {
  width: 33px;
  margin-right: 7px;
}
.main .main-box .grid-2 .tabs1 .bd1 .ul-1 .ul-1-content .ul-1-list .page-btn .active {
  color: #fff;
  background: #000;
  border-color: #000;
}
/*底部样式*/
.footer {
  background: #18292c;
}
.footer .content {
  padding: 36px 40px 0;
}
.footer .content .nav li {
  float: left;
  margin-right: 160px;
}
.footer .content .nav li p {
  font-size: 24px;
  line-height: 24px;
  color: #8c9496;
  margin-bottom: 18px;
}
.footer .content .nav li a {
  font-size: 16px;
  line-height: 30px;
  color: #48666a;
  padding: 5px 0;
  display: block;
}
.footer .content .code {
  border: 1px solid #465456;
  padding: 10px;
}
.footer .content .code img {
  margin-bottom: 8px;
}
.footer .content .code p {
  font-size: 14px;
  line-height: 14px;
  color: #48666a;
  text-align: center;
  margin-top: 4px;
}
.footer .copyright {
  padding: 40px 0;
  font-size: 12px;
  line-height: 20px;
  color: #10434f;
  text-align: center;
}
.fr {
  float: right!important;
}
.fl {
  float: left!important;
}
