body {
  /*font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;*/
  font-family: "微软雅黑";
  font-size: 14px;
  line-height: 1.42857143;
  color: #333;
  background-color: #fff;
}
body,
html {
  min-width: 1200px;
  overflow-x: auto;
}
.mg0 {
  margin: 0!important;
}
.mr42 {
  margin-right: 35px;
}
.mr12 {
  margin-right: 12px;
}
h3 {
  font-weight: normal;
}
.bdno {
  border: none!important;
}
.pt20 {
  padding-top: 20px;
}
.clearfloat:after {
  display: block;
  content: '';
  height: 0;
  width: 0;
  clear: both;
}
.container {
  width: 1200px;
  padding-right: 10px;
  padding-left: 10px;
  margin-right: auto;
  margin-left: auto;
  position: relative;
}

/*内容*/
.main {
  background: #2A2A2A;
}
.main .main-box {
  background: url(../../images/shijiao/bg9.png) center no-repeat;
  background-size: 100% 100%;
  padding: 30px 0;
}
.main .main-box .main-title p {
  font-size: 28px;
  line-height: 60px;
  background: #D7A13D;
  color: white;
  padding: 15px 30px;
}
.main .main-box .main-content {
  background: white;
  padding: 9px 20px 0 20px;
}
.main .main-box .main-content .main-conent-box {
  border-top: 1px solid #F2E9D9;
}
.main .main-box .main-content .main-conent-box ul {
  padding: 36px 28px 0 28px;
}
.main .main-box .main-content .main-conent-box ul li {
  box-sizing: border-box;
  width: 244px;
  float: left;
}
.main .main-box .main-content .main-conent-box ul li .main-conent-box-1 {
  width: 244px;
}
.main .main-box .main-content .main-conent-box ul li .main-conent-box-1 img,
.main .main-box .main-content .main-conent-box ul li .main-conent-box-1 p,
.main .main-box .main-content .main-conent-box ul li .main-conent-box-1 span {
  display: block;
}
.main .main-box .main-content .main-conent-box ul li .main-conent-box-1 img {
  height: 341px;
  width: 244px;
}
.main .main-box .main-content .main-conent-box ul li .main-conent-box-1 .main-conent-box-1-p {
  padding-left: 5px;
}
.main .main-box .main-content .main-conent-box ul li .main-conent-box-1 .main-conent-box-1-p .main-conent-box-1-title {
  font-size: 20px;
  line-height: 38px;
  color: #D7A13D;
}
.main .main-box .main-content .main-conent-box ul li .main-conent-box-1 .main-conent-box-1-p span {
  font-size: 12px;
  line-height: 22px;
  color: #ADADAD;
}
.main .main-box .main-content .main-conent-box ul li .main-conent-box-1 .main-conent-box-1-p .main-conent-box-1-dy {
  font-size: 12px;
  line-height: 22px;
  color: black;
}
.main .main-box .main-content .main-conent-box ul li .main-conent-box-1 .main-conent-box-1-p .main-conent-box-1-dy i {
  color: #D7A13D;
}
.main .main-box .main-content .main-conent-box .main-content-btn {
  width: 335px;
  margin: 0 auto;
  margin-top: 110px;
  padding-bottom: 100px;
  font-size: 12px;
  color: #666666;
}
.main .main-box .main-content .main-conent-box .main-content-btn i,
.main .main-box .main-content .main-conent-box .main-content-btn span {
  display: block;
  float: left;
  border: 1px solid #CCCCCC;
  text-align: center;
  line-height: 33px;
  font-weight: 400;
  box-sizing: border-box;
  cursor: pointer;
}
.main .main-box .main-content .main-conent-box .main-content-btn i {
  width: 33px;
  font-size: 20px;
}
.main .main-box .main-content .main-conent-box .main-content-btn span {
  width: 33px;
}
.main .main-box .main-content .main-conent-box .main-content-btn .active1 {
  border-color: #303030;
  background: #303030;
  color: white;
}
/*底部样式*/
.footer {
  background: #18292c;
}
.footer .content {
  padding: 36px 40px 0;
}
.footer .content .nav li {
  float: left;
  margin-right: 160px;
}
.footer .content .nav li p {
  font-size: 24px;
  line-height: 24px;
  color: #8c9496;
  margin-bottom: 18px;
}
.footer .content .nav li a {
  font-size: 16px;
  line-height: 30px;
  color: #48666a;
  padding: 5px 0;
  display: block;
}
.footer .content .code {
  border: 1px solid #465456;
  padding: 10px;
}
.footer .content .code img {
  margin-bottom: 8px;
}
.footer .content .code p {
  font-size: 14px;
  line-height: 14px;
  color: #48666a;
  text-align: center;
  margin-top: 4px;
}
.footer .copyright {
  padding: 40px 0;
  font-size: 12px;
  line-height: 20px;
  color: #10434f;
  text-align: center;
}
.fr {
  float: right;
}
.fl {
  float: left;
}
