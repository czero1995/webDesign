body {
  /*font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;*/
  font-family: "微软雅黑";
  font-size: 14px;
  line-height: 26px;
  color: #333;
  background-color: #fff;
}
body,html{min-width: 1200px;overflow-x:auto;}

/*表单默认属性*/
input::-webkit-input-placeholder{
  color: #cccccc;opacity:1;
}
textarea::-webkit-input-placeholder{
  color: #cccccc;opacity:1;
}

input,textarea{
  border-color:#CCCCCC;
  font-size: 14px;
  line-height: 26px;
  resize: none;
  outline:none;
  border:1px solid #CCCCCC;
  font-family:微软雅黑;
  text-indent: 10px;
}
textarea:focus{
  border-color:#D6A003;
}
input:focus{
  border-color:#D6A003;
}

.mg0{margin: 0!important;}
h3{font-weight: normal}
.mr40{margin-right:40px;}
.pt15{padding-top: 15px}
.mr18{margin-right:18px;}
.mr10{margin-right:10px;}
.mr7{margin-right:7px;}
.ml12{margin-left:12px;}
.pb30{padding-bottom: 30px}
.bdno{
  border:none!important;}
.clearfloat:after{display: block;content: '';
  height: 0;
  width: 0;clear: both;}

.container {
  width: 1200px;
  padding-right: 10px;
  padding-left: 10px;
  margin-right: auto;
  margin-left: auto;
  position: relative;
}


/*头部*/
.header{
  background: #1f1f1f; padding: 18px 0; z-index: 9999;
  .logo{
    position: absolute; left:10px; top:50%; margin-top: -15px;
  }
  .nav{
    padding-left: 120px; padding-top: 16px;
    li{
      float:left;
      a{
        font-size: 16px; line-height: 28px; color:#989898; padding: 0 15px; margin:0 5px;
      }
    }
  }
  .divLink{
    position: absolute; right:10px; top:50%; margin-top:-22px;
    a{
      position: relative;
    }
    h3{
      font-size: 16px; line-height: 26px; color:#d9ab2b; font-weight: normal;
      span{
        display: inline-block; vertical-align: middle; width:10px; height: 6px; margin-left: 10px; background: url(../../images/ico_down.png) center no-repeat;
      }
    }
    p{
      font-size: 12px; line-height: 18px; color:#999;
    }
    i{
      position: absolute; width:42px; height: 42px; border-radius: 50%; overflow: hidden; left:-50px; top:1px; background: url(../../images/img_01.jpg) center no-repeat;
    }
  }
}
.header.active {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  background: rgba(31,31,31,0.9);
  /* transition: all 0.8s ease 0s; */
  opacity: 0;
  filter: alpha(opacity=0);
  -webkit-transform: translate(0, -80px);
  -moz-transform: translate(0, -80px);
  -ms-transform: translate(0, -80px);
  -o-transform: translate(0, -80px);
  transform: translate(0, -80px);
}
.header.selected {
  opacity: 1;
  filter: alpha(opacity=100);
  -webkit-transition: all 0.8s ease 0s;
  -moz-transition: all 0.8s ease 0s;
  -ms-transition: all 0.8s ease 0s;
  -o-transition: all 0.8s ease 0s;
  transition: all 0.8s ease 0s;
  -webkit-transform: translate(0, 0);
  -moz-transform: translate(0, 0);
  -ms-transform: translate(0, 0);
  -o-transform: translate(0, 0);
  transform: translate(0, 0px);
}


/*内容*/
.main{
  background: #2A2A2A;
  .main-box{
    background: url(../../images/model-homepagebg.png) center no-repeat;
    background-size: 100% 100%;
    background-position-y: 15px;
    .user{
      padding-top: 30px;
      .user-left{
        width: 875px;
        height: 560px;
        float: left;
        background: #000;
        .slideBox{ width:875px; height:560px;  position:relative; border:1px solid transparent;  }
        .slideBox .hd{ height:71px;  position:absolute; right:80px; bottom:30px; z-index:1; }
        .slideBox .hd ul{ overflow:hidden; zoom:1; float:left;  }
        .slideBox .hd ul li{ float:left; width:111px; height:71px; line-height:14px; text-align:center; background:#fff; cursor:pointer;}
        .slideBox .hd ul li img{width: 100%;height: 100%;}
        .slideBox .hd ul li.on{ padding: 2px ;box-sizing: border-box }
        .slideBox .bd{ position:relative; z-index:0; width:555px; height:367px;margin: 0 auto;margin-top: 40px}
        .slideBox .bd li{ zoom:1; vertical-align:middle; }
        .slideBox .bd img{ width:555px; height:367px; display:block;  }

        /* 下面是前/后按钮代码，如果不需要删除即可 */
        .slideBox .prev,
        .slideBox .next{ position:absolute; left:3%; top:40%; margin-top:-25px; display:block; width:32px; height:40px; filter:alpha(opacity=50);opacity:0.5;   }
        .slideBox .next{ left:auto; right:3%; background:url(../../images/ico_next.png) no-repeat;}
        .slideBox .prev{ background:url(../../images/ico_prev.png) no-repeat;}
        .slideBox .prev:hover,
        .slideBox .next:hover{ filter:alpha(opacity=100);opacity:1;  }
        .slideBox .prevStop{ display:none;  }
        .slideBox .nextStop{ display:none;  }
      }
      .user-right{
        width: 325px;
        height: 560px;
        float: left;
        background: white;
        .user-right-top{
          position: relative;
          padding: 20px;
          box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.3);
          .user-right-top1{
          .user-head{
            display: block;
            float: left;
          }
          .user-name{
            padding: 5px 0 0 20px;
            float: left;
            .user-id{
              font-size:16px;
              color: #D7A13B;
              img{
                float: right;
                position: relative;
                top:5px;
                left:-12px;
              }
            }
            .user-company{
              background: url('../../images/model-homepage(thirdparty)/companybg.png');
              background-size: 100% 100%;
              color: #4D737D;
              padding: 5px;
              padding-right: 13px;
              margin: 6px 0;
              span{
                color: #8BD1E2;
              }
            }
            .user-address{
              color: #414141;
              img{
                display: inline-block;
                margin-right: 8px;
                position: relative;
                top:2px;
              }
            }
          }
        }
          .user-right-top2{
            margin-top: 25px;
            .user-right-top2-box{
              font-size:12px;
              color: #589DAF;
              border-left:2px solid #589DAF;
              padding-left: 10px;
              float: left;
            }
          }
          .user-right-top3{
            margin-top: 30px;
            .user-right-top3-box{
              font-size:12px;
              .user-right-top3-img{
                float: left;
                img{
                  display: inline-block;
                  margin-right: 8px;
                  position: relative;
                  top:2px;
                }
                span{
                  color: #D7A13B;
                }
              }
              .last-img{
                position: relative;
                left: -3px;
              }
            }
          }
        }
        .user-right-bottom{
          p{
            font-size:16px;
            color: #816633;
            text-align: center;
            margin-top: 20px;
          }
          .yuyue{
            width: 252px;
            line-height: 46px;
            display: block;
            font-size:16px;
            text-align: center;
            border: 2px solid #D2A929;
            margin: 0 auto;
            color: #D2A929;
            margin-top: 30px;
            cursor: pointer;
            border-radius:2px;
          }
          .yuyue:active{
            border: 2px solid #B38608;
            color: #B38608;
          }
          .shoufei{
            width: 252px;
            line-height: 46px;
            display: block;
            font-size:16px;
            text-align: center;
            border: 2px solid #D6A003;
            border-radius:2px;
            margin: 0 auto;
            color: white;
            background: #D6A003;
            margin-top: 15px;
            cursor: pointer;
          }
          .shoufei:active{
            background: #B38608;
            border: 2px solid #B38608;
          }
          .share{
            width: 150px;
            margin: 0 auto;
            margin-top: 28px;
            margin-bottom: 10px;
            .share-box{
              color: #D0A74B;
              float: left;
              img{
                position: relative;
                top:3px;
                display: inline-block;
                margin-right: 8px;
              }
            }
          }
        }
      }
    }
    .model-instroduce{
      background: white;
      margin-top: 20px;
      padding: 10px 20px;
      .model-instroduce-left{
        float: left;
        .model-instroduce-left-box{
          width: 815px;
          .model-instroduce-left-box-title{
            font-size:16px;
            line-height: 45px;
            color: #B0833A;
            border-bottom: 2px solid #B0833A;
            width: 66px;
            margin-left: 10px;
          }
          .left-box-hr{
            height: 1px;
            background: #EFE5D4;
            position: relative;
            top:-1px;
          }
          .left-box-p{
            margin-top: 25px;
            padding: 0 10px;
            margin-bottom: 10px;
            div{
              .left-box-date{
                float: left;
                color: #D9A84B;
                margin-bottom: 20px;
              }
              .left-box-p-con{
                float: right;
                width: 635px;
                margin-bottom: 15px;
              }
            }
          }
        }
      }
      .model-instroduce-right{
        float: right;
        .TAdevideo{
          .TAdevideo-title{
            color: #D6A003;
            font-size:16px;
            border-left: 2px solid #D6A003;
            padding-left: 15px;
            margin-bottom: 15px;
            margin-top: 10px;
            span{
              display: inline-block;
              float: right;
              font-size:14px;
            }
          }
          .video{
            position: relative;
            img,span{
              display: block;
            }
            .video-time{
              color: white;
              background: rgba(0,0,0,.3);
              font-size:12px;
              padding:0 5px;
              border-radius:5px;
              transform: scale(0.8);
              position: absolute;
              top:10px;
              left: 12px;
            }
            .video-next{
              color: white;
              position: absolute;
              right: 10px;
              top:55px;
              font-size:30px;
            }
            .video-prev{
              color: white;
              position: absolute;
              left: 10px;
              top:55px;
              font-size:30px;
            }
            .video-play{
              position: absolute;
              top:48px;
              left:110px;
            }
          }
          .TAdeID-content{
            padding-top: 15px;
            span{
              display: inline-block;
              width: 73px;
              line-height: 24px;
              text-align: center;
              background: #363636;
              color: white;
              border-radius:5px;
            }
          }
        }
      }
    }
    .main-3{
      background: white;
      margin-top: 20px;
      padding: 10px 20px;
      .main-title{
        color: #B0833A;
        font-size:16px;
        text-indent: 10px;
        line-height: 45px;
      }
      .bd{
        width: 65px;
        height: 2px;
        background: #B0833A;
        margin-left: 10px;
      }
      .hr{
        height: 1px;
        background: #EFE5D4;
        position: relative;
        top: -1px;
      }
      .main-content{
        margin-top: 30px;
        margin-bottom: 25px;
        .main-content1{
          position: relative;
          float: left;
          img{
            display: block;
          }
          .img-info{
            position: absolute;
            bottom:10px;
            left:12px;
            font-size:12px;
            line-height: 22px;
            color: white;
            img{
              float: left;
              border-radius:50%;
            }
            .user-pic{
              margin-right: 8px;
              position: relative;
              top:5px;
            }
            div{
              float: left;
              .user-hot{
                span{
                  display: block;
                  float: left;
                }
                img{
                  float: left;
                }
              }
            }
          }
          .img-shadow{
            position: absolute;
            width: 150px;
            height: 215px;
            top:0;
            left:0;
            box-shadow: 0px -25px 20px rgba(0, 0, 0, 0.5) inset;
          }
        }
      }
      .comment{
        padding: 0 30px;
        li{
          padding: 35px 0;
          border-bottom: 1px solid #E6E6E6;
          .comment-pic{
            display: block;
            float: left;
            width: 65px;
            height: 65px;
            margin-right: 20px;
          }
          .comment-content{
            float: left;
            width: 1010px;
            .comment-title{
              margin-bottom: 20px;
              p{
                color: #B0833A;
                font-size:16px;
                float: left;
                margin-right: 60px;
              }
              span{
                display: block;
                float: left;
                line-height: 26px;
                position: relative;
                img{
                  position: absolute;
                  display: inline-block;
                  left: -25px;
                  top:0px;
                }
              }
              i{
                display: block;
                float: right;
                color: #D8D8D8;
                font-size:12px;
                line-height: 26px;
              }
            }
          }
        }
      }
      .comment-btn{
        width: 340px;
        margin: 0 auto;
        margin-top: 68px;
        margin-bottom: 60px;
        i,span{
          display: block;
          float: left;
          color: #666666;
          border: 1px solid #CCCCCC;
          text-align: center;
          cursor: pointer;
        }
        i{
          width: 53px;
          line-height: 33px;
          font-size:20px;
        }
        span{
          width: 33px;
          line-height: 33px;
        }
        .active1{
          background: #303030;
          color: white;
          border-color:#303030;
        }
      }
    }
  }
}

/*底部样式*/
.footer{
  background: #18292c;
  .content{
    padding:36px 40px 0;
    .nav{
      li{
        float:left; margin-right: 160px;
        p{
          font-size: 24px; line-height: 24px; color:#8c9496; margin-bottom: 18px;
        }
        a{
          font-size: 16px; line-height: 30px; color:#48666a; padding: 5px 0;display: block;
        }
      }
    }
    .code{
      border: 1px solid #465456; padding:10px;
      img{
        margin-bottom: 8px;
      }
      p{
        font-size: 14px; line-height: 14px; color:#48666a; text-align: center; margin-top: 4px;
      }
    }
  }
  .copyright{
    padding:40px 0; font-size: 12px; line-height: 20px; color:#10434f; text-align: center;
  }
}
.fr{
  float: right;}
.fl{
  float: left;
}