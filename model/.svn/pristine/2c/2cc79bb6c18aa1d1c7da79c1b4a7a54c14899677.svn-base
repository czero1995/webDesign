/*公用*/
.section .en { text-transform: uppercase;  }
.section .title_1 { margin-bottom: 20px; }
.section .title_1 h3 { font-size: 32px; line-height: 32px; color:#b58933; letter-spacing: 7px; margin-bottom: 6px; }
.section .title_1 p { font-size: 12px; line-height: 12px; color:#b58933; }
.section .title_2 { text-align: center; margin-bottom: 20px; }
.section .title_2 h3 { font-size: 20px; line-height: 20px; color:#c6a057; margin-bottom: 8px; letter-spacing: 1px; }
.section .title_2 p { font-size: 12px; line-height: 12px; color:#808080;  }
.section .nav {  }
.section .nav span { display: inline-block; vertical-align: middle; font-size: 16px; line-height: 16px; color:#a3a3a3; padding-bottom: 14px; margin:0 18px; }
.section .nav a { display: inline-block; vertical-align: middle; font-size: 0; color:#a3a3a3; position: relative; padding-right: 24px; margin-right: 18px; }
.section .nav a::after { position: absolute; top:0; right:0; content:"/"; font-size: 16px; line-height: 16px; color:#a3a3a3; }
.section .nav a.selected,.section .nav a:hover { color:#b58933; }
.section .nav a p { font-size: 16px; line-height: 16px; }
.section .nav a p.en { font-size: 12px; line-height: 12px; text-align: center; margin-top: 2px; letter-spacing: 0; }

.section .top .nav { display: inline-block; vertical-align: middle; }
.section .top .more { display: inline-block; vertical-align: middle; margin-left: 60px; }
.section .top .more:hover {  background: #c5ab3c;  }
.section .top .more a { font-size: 14px; line-height: 26px; color:#c5ab3c; text-align: center; width: 138px; border: 2px solid #c5ab3c; border-radius: 4px; }
.section .top .more a:hover{color:#fff;}
.section .top .fr { padding: 10px 0 0; }

.character { position: relative; padding:1px 0; padding-left: 45px; }
.character i { position: absolute; width:38px; height: 38px; left:0; top:0; border-radius: 50%; overflow: hidden; background: url(../images/img_02.jpg); background-size:cover; }
.character h3 { font-size: 14px; line-height: 18px; font-weight: normal; margin-bottom: 2px; }
.character p { font-size: 14px; line-height: 16px; padding-left: 22px;  }
.character span { position: absolute; right:0; top:50%; font-size: 14px; line-height: 16px; color:#cbad42; padding-left: 22px; margin-top: -8px; background: url(../images/ico_32.png) left center no-repeat;  }

.tabsBox { display: none; }
.tabsBox:first-child { display: block; }

.scrollbar { height: 100%; overflow: hidden; overflow-y: auto; }
/*定义滚动条宽高及背景，宽高分别对应横竖滚动条的尺寸*/
.scrollbar::-webkit-scrollbar{
    width: 8px;
    height: 16px;
    background-color: #f5f5f5;
}
/*定义滚动条的轨道，内阴影及圆角*/
.scrollbar::-webkit-scrollbar-track{
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
    border-radius: 10px;
    background-color: #f5f5f5;
}
/*定义滑块，内阴影及圆角*/
.scrollbar::-webkit-scrollbar-thumb{
    /*width: 10px;*/
    height: 20px;
    border-radius: 10px;
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,.3);
    background-color: #133237;
}

