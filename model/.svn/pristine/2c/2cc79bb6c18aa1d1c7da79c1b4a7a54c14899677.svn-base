* {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
/*banner*/
.banner { position: relative; height: 640px; }
.banner .slideTxtBox .bd li,.banner .slideTxtBox .bd li a { height: 640px; }
.banner .slideTxtBox .bd img { width:100%; }

.banner .slideTxtBox { position: relative; }
.banner .slideTxtBox .bd { position: relative; z-index: 98; }
.banner .slideTxtBox .hd { position: absolute; z-index: 99; left:50%; margin-left:-100px; bottom:120px;  }
.banner .slideTxtBox .hd li { float:left; width:12px; height: 12px; border-radius: 50%; overflow: hidden; background: #8b8e8f; margin:0 20px; cursor: pointer; }
.banner .slideTxtBox .hd li.on { background: #c3a05e; }
.banner .slideTxtBox .container { position: absolute; left:0; right:0; height: 100%; top:0; }
.banner .slideTxtBox .lr { position: absolute; z-index: 99; top: 50%; margin-top: -32px; display: block; width: 64px; height: 64px; border-radius: 50%; filter: alpha(opacity=50); opacity: 0.5;}
.banner .slideTxtBox .lr:hover { background-color: rgba(255,255,255,.1);filter: alpha(opacity=100); opacity: 1; }
.banner .slideTxtBox .prev { left:0; background: url(../images/ico_prev.png) center no-repeat; }
.banner .slideTxtBox .next { right:0; background: url(../images/ico_next.png) center no-repeat; }

.banner .main { position: absolute; z-index: 99; top:50%; left:50%; transform:translate(-49%,-60%); -ms-transform:translate(-49%,-60%); -o-transform:translate(-49%,-60%); -moz-transform:translate(-49%,-60%); -webkit-transform:translate(-49%,-60%); }
.banner .main h3 { font-size: 62px; line-height: 62px; color:#fff; letter-spacing: 20px; font-weight: normal; word-break:keep-all; white-space:nowrap; margin-bottom: 40px; }
.banner .main .search { position: relative; padding-right: 20px; }
.banner .main .search input { width:100%; font-size: 20px; color:#fff; line-height: 24px; padding: 20px 40px; width:100%; border: none; border-radius: 32px; background: rgba(0,0,0,.56); }
.banner .main .search i { position: absolute; width: 60px; height: 60px; top:50%; margin-top: -30px; right:40px; background: url(../images/ico_search.png) center no-repeat; cursor: pointer; }
.banner .bottom { position: absolute; z-index: 99; left:0; bottom:0; width:100%; }
.banner .bottom img { width:100%; }


