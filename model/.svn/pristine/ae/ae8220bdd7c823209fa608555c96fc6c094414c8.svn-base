body {
  /*font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;*/
  font-family: "PingFang SC", "微软雅黑", "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 14px;
  line-height: 26px;
  color: #333;
  background-color: #fff;
}
body,html{min-width: 1200px;overflow-x:auto;}

/*表单默认属性*/
input::-webkit-input-placeholder{
  color: #cccccc;opacity:1;
}
textarea::-webkit-input-placeholder{
  color: #cccccc;opacity:1;
}

input,textarea{
  border-color:#CCCCCC;
  font-size: 14px;
  line-height: 26px;
  resize: none;
  outline:none;
  border:1px solid #CCCCCC;
  text-indent: 10px;
}
textarea:focus{
  border-color:#D6A003;
}
input:focus{
  border-color:#D6A003;
}

.mg0{margin: 0!important;}
.mr12{margin-right: 12px;}
h3{font-weight: normal}
.bdno{
  border:none!important;}
.clearfloat:after{display: block;content: '';
  height: 0;
  width: 0;clear: both;}

.container {
  width: 1200px;
  padding-right: 10px;
  padding-left: 10px;
  margin-right: auto;
  margin-left: auto;
  position: relative;
}


/*头部*/
.header{
  background: #1f1f1f; padding: 18px 0; z-index: 9999;
  .logo{
    position: absolute; left:10px; top:50%; margin-top: -15px;
  }
  .nav{
    padding-left: 120px; padding-top: 16px;
    li{
      float:left;
      a{
        font-size: 16px; line-height: 28px; color:#989898; padding: 0 15px; margin:0 5px;
      }
    }
  }
  .divLink{
    position: absolute; right:10px; top:50%; margin-top:-22px;
    a{
      position: relative;
    }
    h3{
      font-size: 16px; line-height: 26px; color:#d9ab2b; font-weight: normal;
      span{
        display: inline-block; vertical-align: middle; width:10px; height: 6px; margin-left: 10px; background: url(../../../images/ico_down.png) center no-repeat;
      }
    }
    p{
      font-size: 12px; line-height: 18px; color:#999;
    }
    i{
      position: absolute; width:42px; height: 42px; border-radius: 50%; overflow: hidden; left:-50px; top:1px; background: url(../../../images/img_01.jpg) center no-repeat;
    }
  }
}
.header.active {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  background: rgba(31,31,31,0.9);
  /* transition: all 0.8s ease 0s; */
  opacity: 0;
  filter: alpha(opacity=0);
  -webkit-transform: translate(0, -80px);
  -moz-transform: translate(0, -80px);
  -ms-transform: translate(0, -80px);
  -o-transform: translate(0, -80px);
  transform: translate(0, -80px);
}
.header.selected {
  opacity: 1;
  filter: alpha(opacity=100);
  -webkit-transition: all 0.8s ease 0s;
  -moz-transition: all 0.8s ease 0s;
  -ms-transition: all 0.8s ease 0s;
  -o-transition: all 0.8s ease 0s;
  transition: all 0.8s ease 0s;
  -webkit-transform: translate(0, 0);
  -moz-transform: translate(0, 0);
  -ms-transform: translate(0, 0);
  -o-transform: translate(0, 0);
  transform: translate(0, 0px);
}


/*内容*/
.main{
  background: #fff;
  .main-box{
    background: url("../../../images/personal-homepage/bg.png") no-repeat;
    background-size: 100% auto;
    padding-top: 38px;
    .grid-1{
      background: #303030;
      padding: 45px 75px 25px 45px;
      position: relative;
      .head-img{
        position: relative;
        width: 115px;
        height: 115px;
        float: left;
        img{
          height: 100%;
          width: 100%;
          display: block;
          border-radius:50%;
        }
        i.v{
          width: 23px;
          height: 23px;
          display: block;
          position: absolute;
          background: url("../../../images/personal-homepage/shiming.png") no-repeat;
          background-size: 100% 100%;
          right: 12px;
          bottom:0;
        }
      }
      .personal-info{
        padding: 25px 20px;
        float: left;
        .user-name{
          font-size:20px;
          color: #C29F4A;
          margin-bottom: 20px;
          span{
            width: 67px;
            height: 25px;
            background: url("../../../images/personal-homepage/pthuiyuan.png") no-repeat;
            background-size: 100% 100%;
            border-radius:20px;
            display: inline-block;
            margin-right: 10px;
            position: relative;
            top:5px;
          }
          .sex{
            width: 16px;
            height: 19px;
            display: inline-block;
            background: url("../../../images/personal-homepage/women.png") no-repeat;
            background-size: 100% 100%;
            margin-left: 20px;
            position: relative;
            top:3px;
          }
        }
        .user-info{
          .user-info-unit{
            display: block;
            float: left;
            color: #CCCCCC;
            margin-right: 30px;
            i{
              position: relative;
              top:2px;
              margin-right: 5px;
            }
            .address{
              display: inline-block;
              width: 13px;
              height: 17px;
              background: url("../../../images/personal-homepage/address.png") no-repeat;
              background-size: 100% 100%;
            }
            .star{
              display: inline-block;
              width: 18px;
              height: 18px;
              background: url("../../../images/personal-homepage/star.png") no-repeat;
              background-size: 100% 100%;
            }
            .hot{
              display: inline-block;
              width: 13px;
              height: 17px;
              background: url("../../../images/personal-homepage/hot.png") no-repeat;
              background-size: 100% 100%;
            }
            .gift{
              display: inline-block;
              width: 17px;
              height: 15px;
              background: url("../../../images/personal-homepage/gift.png") no-repeat;
              background-size: 100% 100%;
            }
          }
        }
      }
      .sign-in-btn{
        width: 154px;
        height: 50px;
        line-height: 50px;
        color: #fff;
        border: 2px solid #fff;
        border-radius:30px;
        text-align: center;
        cursor: pointer;
        position: absolute;
        top:0;
        bottom:0;
        right: 75px;
        margin: auto;
        .sign-in{
          width: 24px;
          height: 24px;
          display: inline-block;
          position: relative;
          top:5px;
          background: url("../../../images/personal-homepage/sign-in.png") no-repeat;
          background-size: 100% 100%;
          margin-right: 12px;
        }
      }
    }
    .grid-2{
      padding: 30px 0;
      .tabs1{
        .hd1{
          width: 208px;
          float: left;
          background: #fff;
          margin-right: 10px;
          ul{
            border: 1px solid #E6E6E6;
            border-bottom:0;
            li{
              line-height: 66px;
              font-size:16px;
              border-bottom: 1px solid #E6E6E6;
              position: relative;
              cursor: pointer;
              a{
                padding-left: 49px;
                display: block;
                width: 100%;
                height: 100%;
                box-sizing: border-box;
              }
            }
            .on{
              background: #000;
              a{
                color: #CBAD50;
              }
              .right{
                width: 10px;
                height: 15px;
                background: url("../../../images/about-us/right.png") no-repeat;
                background-size: 100% 100%;
                position: absolute;
                top:0;
                bottom:0;
                right:25px;
                margin: auto;
              }
            }
          }
        }
        .bd1{
          width: 975px;
          float: right;
          .ul-1{
            border: 1px solid #E6E6E6;
            .ul-1-title{
              height: 147px;
              border-bottom: 1px solid #E6E6E6;
              padding-left:74px;
              padding-top: 30px;
              background: #FAFAFA;
              box-sizing: border-box;
              .u-plan:nth-of-type(1){
                .u-plan-content{
                  margin-left: 0;
                  padding-left: 0px;
                }
              }
              .u-plan{
                float: left;
                position: relative;
                .hr{
                  float: left;
                  border-bottom: 1px dashed #ccc;
                  height: 0;
                  width: 60px;
                  position: absolute;
                  top:25px;
                  left: 0;
                  margin: auto;
                }
                .u-plan-content {
                  float: left;
                  margin-right:20px;
                  padding-left: 80px;
                  .u-plan-img{
                    width: 52px;
                    height: 52px;
                    margin: 0 auto;
                    margin-bottom: 5px;
                    background-size: 100% 100%;
                  }
                  p{
                    color: #A1A1A1;
                  }
                  .u-plan-1{
                    background: url("../../../images/personal-homepage/manage-annunciate/pic1-2.png");
                  }
                  .u-plan-2{
                    background: url("../../../images/personal-homepage/manage-annunciate/pic2-2.png");
                  }
                  .u-plan-3{
                    background: url("../../../images/personal-homepage/manage-annunciate/pic3-2.png");
                  }
                  .u-plan-4{
                    background: url("../../../images/personal-homepage/manage-annunciate/pic4-2.png");
                  }
                  .u-plan-5{
                    background: url("../../../images/personal-homepage/manage-annunciate/pic5-2.png");
                  }
                  .u-plan-6{
                    background: url("../../../images/personal-homepage/manage-annunciate/pic6-2.png");
                  }
                }
              }
              .active{
                .hr{
                  border-bottom: 1px dashed #DFAB44;
                }
                .u-plan-content{
                  p{
                    color: #DFAB44;
                  }
                  .u-plan-1{
                    background: url("../../../images/personal-homepage/manage-annunciate/pic1-1.png");
                  }
                  .u-plan-2{
                    background: url("../../../images/personal-homepage/manage-annunciate/pic2-1.png");
                  }
                  .u-plan-3{
                    background: url("../../../images/personal-homepage/manage-annunciate/pic3-1.png");
                  }
                  .u-plan-4{
                    background: url("../../../images/personal-homepage/manage-annunciate/pic4-1.png");
                  }
                  .u-plan-5{
                    background: url("../../../images/personal-homepage/manage-annunciate/pic5-1.png");
                  }
                  .u-plan-6{
                    background: url("../../../images/personal-homepage/manage-annunciate/pic6-1.png");
                  }
                }
              }
            }
            .ul-1-content{
              padding-top: 25px;
              background: #fff;
              padding-left: 45px;
              padding-right: 45px;
              .content-title{
                margin-bottom: 10px;
                .u-1{
                  float: left;
                  .title{
                    font-size:18px;
                    span{
                      font-size:16px;
                      color: #B3B3B3;
                      margin-right: 22px;
                    }
                    i.flag{
                      display: inline-block;
                      width: 14px;
                      height: 19px;
                      background-size: 100% 100%;
                      margin-right: 7px;
                      position: relative;
                      top:4px;
                    }
                    em{
                      font-size:16px;
                    }
                    .red{
                      color: #EB2C40;
                      .flag{
                        background: url("../../../images/personal-homepage/manage-annunciate/redflag.png") no-repeat;
                      }
                    }
                  }
                  .time{
                    color: #B3B3B3;
                    font-size:12px;
                  }
                }
                .u-2{
                  float: right;
                  p{
                    font-size:12px;
                    letter-spacing: 2px;
                  }
                  .over-time{
                    margin: 0 auto;
                    width: 84px;
                    height: 46px;
                    background: url("../../../images/personal-homepage/manage-annunciate/over-time(2).png") no-repeat;
                    background-size: 100% 100%;
                    line-height: 46px;
                    text-align: center;
                    font-size:22px;
                    color: #DFAA43;
                  }
                }
              }
              .mod-1{
                margin-bottom: 45px;
                p{
                  font-size:16px;
                  text-align: center;
                  width:65px;
                  border-bottom: 1px solid #DEA636;
                  color: #DEA636;
                  line-height: 20px;
                  margin: 0 auto;
                  position: relative;
                  margin-bottom: 25px;
                  i.down{
                    display: block;
                    width: 15px;
                    height: 16px;
                    background: url("../../../images/personal-homepage/manage-annunciate/down.png") no-repeat;
                    background-size:  100% 100%;
                    position: absolute;
                    right: -30px;
                    top:5px;
                  }
                }
                .u-info{
                  padding-bottom: 20px;
                  border-bottom: 2px solid #DEA228;
                  span{
                    color: #E2AD3A;
                  }
                }
                .mod-1-list{
                  ul{
                    margin-bottom: 18px;
                    li{
                      height: 46px;
                      line-height: 46px;
                      border-bottom: 1px dashed #E6E6E6;
                      span{
                        color: #E2AD3A;
                      }
                    }
                  }
                }
                .mod-1-label{
                  span{
                    width: 72px;
                    line-height: 20px;
                    text-align: center;
                    display: block;
                    float: left;
                    margin-right: 10px;
                    background: #707070;
                    color: #fff;
                    border-radius:3px;
                  }
                }
              }
              .mod-2{
                .mod-2-title{
                  position: relative;
                  margin-bottom: 30px;
                  span{
                    display: block;
                    font-size:16px;
                    width: 80px;
                  }
                  .hr{
                    width: 803px;
                    height: 1px;
                    background: #F0E6D3;
                    position: absolute;
                    top: 0;
                    right:0;
                    bottom:0;
                    margin: auto;
                  }
                }
                .photo{
                  margin-bottom: 9px;
                  .img-box{
                    width: 196px;
                    height: 149px;
                    float: left;
                    margin-right: 24px;
                    margin-bottom: 24px;
                    img{
                      width: 100%;
                      height: 100%;
                      object-fit: cover;
                    }
                  }
                }
              }
              .mod-3{
                .mod-3-title{
                  line-height: 46px;
                  text-align: center;
                  width: 100%;
                  background: #F6B738;
                  font-size:16px;
                }
                .mod-3-content{
                  padding-top: 25px;
                  .u-model{
                    position: relative;
                    margin-bottom: 40px;
                    .head-img{
                      float: left;
                      width: 78px;
                      height: 78px;
                      border-radius:50%;
                      margin-right: 10px;
                      img{
                        width: 100%;
                        height: 100%;
                        border-radius:50%;
                      }
                    }
                    .model-info{
                      float: left;
                      padding-top: 5px;
                      line-height: 20px;
                      .name{
                        margin-bottom: 5px;
                        .women{
                          width: 13px;
                          height: 17px;
                          display: inline-block;
                          background: url("../../../images/personal-homepage/women.png") no-repeat;
                          background-size: 100% 100%;
                          margin-left: 8px;
                          position: relative;
                          top:4px;
                        }
                      }
                      .address{
                        font-size:12px;
                        margin-bottom: 10px;
                      }
                      .model-label{
                        span{
                          display: block;
                          float: left;
                          width: 73px;
                          line-height: 24px;
                          text-align: center;
                          margin-right: 18px;
                          background: #363636;
                          color: #fff;
                          border-radius:3px;
                        }
                      }
                    }
                  }
                }
              }
              .mod-4{
                .mod-4-title{
                  line-height: 46px;
                  text-align: center;
                  width: 100%;
                  background: #F6B738;
                  font-size:16px;
                  margin-bottom: 22px;
                }
                .selcet-box{
                  margin-bottom: 30px;
                  span,.dropdown{
                    display: block;
                    float: left;
                  }
                  span{
                    line-height: 42px;
                    margin-right: 15px;
                  }
                  .dropdown{
                    width: 243px;
                    height: 40px;
                    line-height: 40px;
                    padding-left: 13px;
                    border: 1px solid #E6E6E6;
                    position: relative;
                    .caret{
                      position: absolute;
                      top:0;
                      bottom:0;
                      margin: auto;
                      right: 15px;
                      display: block;
                      width: 0;
                      height: 0;
                      vertical-align: middle;
                      border-top: 6px dashed #999;
                      border-right: 6px solid transparent;
                      border-left: 6px solid transparent;
                    }
                  }
                }
                .mod-4-content{
                  padding-bottom: 45px;
                  .u-model{
                    position: relative;
                    margin-bottom: 35px;
                    .head-img{
                      float: left;
                      width: 62px;
                      height: 62px;
                      border-radius:50%;
                      margin-right: 14px;
                      img{
                        width: 100%;
                        height: 100%;
                        border-radius:50%;
                      }
                    }
                    .model-info{
                      float: left;
                      line-height: 20px;
                      .name{
                        .women{
                          width: 10px;
                          height: 12px;
                          display: inline-block;
                          background: url("../../../images/personal-homepage/women.png") no-repeat;
                          background-size: 100% 100%;
                          margin-left: 8px;
                          position: relative;
                          top:1px;
                        }
                      }
                      .address{
                        font-size:12px;
                        margin-bottom: 5px;
                      }
                      .model-label{
                        span{
                          display: block;
                          float: left;
                          width: 73px;
                          line-height: 24px;
                          text-align: center;
                          margin-right: 18px;
                          background: #363636;
                          color: #fff;
                          border-radius:3px;
                        }
                      }
                    }
                  }
                }
              }
              .page-btn{
                width: 345px;
                margin: 0 auto;
                padding-bottom: 85px;
                i,span{
                  display: block;
                  float: left;
                  text-align: center;
                  line-height: 33px;
                  border: 1px solid #ccc;
                  cursor: pointer;
                }
                i{
                  width: 53px;
                  margin-right: 12px;
                }
                span{
                  width: 33px;
                  margin-right: 7px;
                }
                .active{
                  background: #000;
                  border-color:#000;
                  color: #fff;
                }
              }
              .checked-btn{
                width: 82px;
                height: 28px;
                line-height: 28px;
                text-align: center;
                border: 2px solid #D2A929;
                border-radius:3px;
                color: #D2A929;
                position: absolute;
                top:0;
                bottom:0;
                right: 0;
                margin: auto;
                cursor: pointer;
              }
              .checked-btn:active{
                border-color:#B38608;
                color: #B38608;
              }
            }
          }
        }
      }
    }
  }
}

/*底部样式*/
.footer{
  background: #18292c;
  .content{
    padding:36px 40px 0;
    .nav{
      li{
        float:left; margin-right: 160px;
        p{
          font-size: 24px; line-height: 24px; color:#8c9496; margin-bottom: 18px;
        }
        a{
          font-size: 16px; line-height: 30px; color:#48666a; padding: 5px 0;display: block;
        }
      }
    }
    .code{
      border: 1px solid #465456; padding:10px;
      img{
        margin-bottom: 8px;
      }
      p{
        font-size: 14px; line-height: 14px; color:#48666a; text-align: center; margin-top: 4px;
      }
    }
  }
  .copyright{
    padding:40px 0; font-size: 12px; line-height: 20px; color:#10434f; text-align: center;
  }
}
.fr{
  float: right!important;}
.fl{
  float: left!important;
}