body {
  /*font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;*/
  font-family: "微软雅黑", "Helvetica Neue", Helvetica, "PingFang SC", Arial, sans-serif;
  font-size: 14px;
  line-height: 26px;
  color: #333;
  background-color: #fff;
}
body,html{min-width: 1200px;overflow-x:auto;}

/*表单默认属性*/
input::-webkit-input-placeholder{
  color: #cccccc;opacity:1;
}
textarea::-webkit-input-placeholder{
  color: #cccccc;opacity:1;
}

input,textarea{
  border-color:#CCCCCC;
  font-size: 14px;
  line-height: 26px;
  resize: none;
  outline:none;
  border:1px solid #CCCCCC;
  text-indent: 10px;
}
textarea:focus{
  border-color:#D6A003;
}
input:focus{
  border-color:#D6A003;
}

.mg0{margin: 0!important;}
.mr40{margin-right: 40px;}
h3{font-weight: normal}
.bdno{
  border:none!important;}
.clearfloat:after{display: block;content: '';
  height: 0;
  width: 0;clear: both;}

.container {
  width: 1200px;
  padding-right: 10px;
  padding-left: 10px;
  margin-right: auto;
  margin-left: auto;
  position: relative;
}


/*头部*/
.header{
  background: #1f1f1f; padding: 18px 0; z-index: 9999;
  .logo{
    position: absolute; left:10px; top:50%; margin-top: -15px;
  }
  .nav{
    padding-left: 120px; padding-top: 16px;
    li{
      float:left;
      a{
        font-size: 16px; line-height: 28px; color:#989898; padding: 0 15px; margin:0 5px;
      }
    }
  }
  .divLink{
    position: absolute; right:10px; top:50%; margin-top:-22px;
    a{
      position: relative;
    }
    h3{
      font-size: 16px; line-height: 26px; color:#d9ab2b; font-weight: normal;
      span{
        display: inline-block; vertical-align: middle; width:10px; height: 6px; margin-left: 10px; background: url(../../images/ico_down.png) center no-repeat;
      }
    }
    p{
      font-size: 12px; line-height: 18px; color:#999;
    }
    i{
      position: absolute; width:42px; height: 42px; border-radius: 50%; overflow: hidden; left:-50px; top:1px; background: url(../../images/img_01.jpg) center no-repeat;
    }
  }
}
.header.active {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  background: rgba(31,31,31,0.9);
  /* transition: all 0.8s ease 0s; */
  opacity: 0;
  filter: alpha(opacity=0);
  -webkit-transform: translate(0, -80px);
  -moz-transform: translate(0, -80px);
  -ms-transform: translate(0, -80px);
  -o-transform: translate(0, -80px);
  transform: translate(0, -80px);
}
.header.selected {
  opacity: 1;
  filter: alpha(opacity=100);
  -webkit-transition: all 0.8s ease 0s;
  -moz-transition: all 0.8s ease 0s;
  -ms-transition: all 0.8s ease 0s;
  -o-transition: all 0.8s ease 0s;
  transition: all 0.8s ease 0s;
  -webkit-transform: translate(0, 0);
  -moz-transform: translate(0, 0);
  -ms-transform: translate(0, 0);
  -o-transform: translate(0, 0);
  transform: translate(0, 0px);
}


/*内容*/
.main{
  background: #1f1f1f;
  .main-box{
    background: url("../../images/model-point/bg4.png") no-repeat;
    .main-title{
      p{
        font-size: 20px;
        line-height: 60px;
        background: #D7A13D;
        color: white;
        padding: 15px 30px;
        .flag{
          display: inline-block;
          width: 15px;
          height: 20px;
          margin-left: 30px;
          margin-right: 8px;
          background: url("../../images/activity/flag-icon (2).png") no-repeat;
          background-size: 100% 100%;
          position: relative;
          top:3px;
        }
        .active-time{
          display: block;
          float: right;
          font-size:12px;
          color: #000;
          letter-spacing: 3px;
          em{
            width: 65px;
            height: 35px;
            display: inline-block;
            background: url("../../images/activity/over-time.png") no-repeat;
            background-size: 100% 100%;
            font-size:16px;
            line-height:35px;
            text-align: center;
            color: #fff;
            margin-left: 10px;
          }
        }
      }
    }
    .main-content{
      background: white;
      padding: 9px 20px 0 20px;
      .main-conent-box{
        border-top: 1px solid #F2E9D9;
        .conent-box{
          padding: 45px 10px;
          .active-info{
            margin-bottom: 38px;
            span{
              color: #DAAA4F;
              margin-right: 8px;
            }
          }
          .active-address{
            margin-bottom: 35px;
            .address-info{
              width: 620px;
              float: left;
              ul{
                border-top: 2px solid #D7A13D;
                margin-bottom: 25px;
                li{
                  line-height: 45px;
                  border-bottom: 1px dashed #E6E6E6;
                  span{
                    color: #DAAA4F;
                    margin-right: 8px;
                    display: inline-block;
                    width: 60px;
                    text-align: right;
                  }
                }
              }
              .active-label{
                span{
                  width: 72px;
                  height: 20px;
                  text-align: center;
                  line-height: 20px;
                  background: #707070;
                  color: #fff;
                  display: block;
                  float: left;
                  border-radius:5px;
                  margin-right: 10px;
                }
              }
            }
            .address-map{
              float: right;
              width: 453px;
              height: 322px;
              img{
                display: block;
                width: 100%;
                height: 100%;
              }
            }
          }
          .active-mod3{
            margin-bottom: 50px;
            .mod-title{
              position: relative;
              box-sizing: border-box;
              margin-bottom: 30px;
              span{
                display: block;
                font-size:18px;
                width: 80px;
                float: left;
              }
              .hr{
                position: absolute;
                width: 93%;
                height: 1px;
                background: #EFE5D4;
                top:0;
                bottom:0;
                margin: auto;
                left: 80px;
                right: 0;
              }
            }
            .mod3-img{
              .mod3-img-box{
                width: 255px;
                height: 195px;
                float: left;
                background: url("../../images/activity/active-info.png") no-repeat;
                background-size: 100% 100%;
              }
            }
          }
          .active-mod4{
            margin-bottom: 95px;
            .mod-title{
              position: relative;
              box-sizing: border-box;
              margin-bottom: 45px;
              span{
                display: block;
                font-size:18px;
                width: 120px;
                float: left;
              }
              .hr{
                position: absolute;
                width: 90%;
                height: 1px;
                background: #EFE5D4;
                top:0;
                bottom:0;
                margin: auto;
                right: 0;
              }
            }
            .header-portrait{
              width: 54px;
              height: 54px;
              float: left;
              border-radius:50%;
              background: url("../../images/activity/header-portrait.png") no-repeat;
              background-size: 100% 100%;
              margin-right: 18px;
            }
          }
          .submit{
            width: 216px;
            height: 50px;
            margin: 0 auto;
            background: #D7A13D;
            line-height: 50px;
            text-align: center;
            color: #fff;
            cursor: pointer;
          }
          .submit:active{
            background: #B38608;
          }
        }
      }
    }
  }
}

/*底部样式*/
.footer{
  background: #18292c;
  .content{
    padding:36px 40px 0;
    .nav{
      li{
        float:left; margin-right: 160px;
        p{
          font-size: 24px; line-height: 24px; color:#8c9496; margin-bottom: 18px;
        }
        a{
          font-size: 16px; line-height: 30px; color:#48666a; padding: 5px 0;display: block;
        }
      }
    }
    .code{
      border: 1px solid #465456; padding:10px;
      img{
        margin-bottom: 8px;
      }
      p{
        font-size: 14px; line-height: 14px; color:#48666a; text-align: center; margin-top: 4px;
      }
    }
  }
  .copyright{
    padding:40px 0; font-size: 12px; line-height: 20px; color:#10434f; text-align: center;
  }
}
.fr{
  float: right!important;}
.fl{
  float: left!important;
}