body {
  /*font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;*/
  font-family: "微软雅黑", "Helvetica Neue", Helvetica, "PingFang SC", Arial, sans-serif;
  font-size: 14px;
  line-height: 26px;
  color: #333;
  background-color: #fff;
}
body,
html {
  min-width: 1200px;
  overflow-x: auto;
}
/*表单默认属性*/
input::-webkit-input-placeholder {
  color: #cccccc;
  opacity: 1;
}
textarea::-webkit-input-placeholder {
  color: #cccccc;
  opacity: 1;
}
input,
textarea {
  border-color: #CCCCCC;
  font-size: 14px;
  line-height: 26px;
  resize: none;
  outline: none;
  border: 1px solid #CCCCCC;
  text-indent: 10px;
}
textarea:focus {
  border-color: #D6A003;
}
input:focus {
  border-color: #D6A003;
}
.mg0 {
  margin: 0!important;
}
.mr40 {
  margin-right: 40px;
}
h3 {
  font-weight: normal;
}
.bdno {
  border: none!important;
}
.clearfloat:after {
  display: block;
  content: '';
  height: 0;
  width: 0;
  clear: both;
}
.container {
  width: 1200px;
  padding-right: 10px;
  padding-left: 10px;
  margin-right: auto;
  margin-left: auto;
  position: relative;
}

/*内容*/
.main {
  background: #fff;
}
.main .main-box {
  background: url("../../../images/personal-homepage/bg.png") no-repeat;
  background-size: 100% auto;
  padding-top: 38px;
  padding-bottom: 25px;
}
.main .main-box .content {
  border: 1px solid #E5E5E5;
  background: #fff;
  padding-bottom: 60px;
}
.main .main-box .content .m-title {
  line-height: 78px;
  padding-left: 28px;
  font-size: 18px;
  border-bottom: 1px solid #E5E5E5;
}
.main .main-box .content .m-info {
  padding: 0 40px;
}
.main .main-box .content .m-info .u-model {
  padding: 30px 10px;
  padding-bottom: 25px;
  border-bottom: 1px solid #E5E5E5;
}
.main .main-box .content .m-info .u-model .u-title {
  display: block;
  margin-bottom: 20px;
  font-size: 16px;
}
.main .main-box .content .m-info .u-model .u-modelName .head-img {
  float: left;
  width: 78px;
  height: 78px;
  margin-right: 20px;
}
.main .main-box .content .m-info .u-model .u-modelName .head-img img {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.main .main-box .content .m-info .u-model .u-modelName .model-name {
  float: left;
  padding-top: 10px;
}
.main .main-box .content .m-info .u-model .u-modelName .model-name p.name {
  font-size: 14px;
}
.main .main-box .content .m-info .u-model .u-modelName .model-name p.name .sex {
  width: 16px;
  height: 19px;
  display: inline-block;
  background: url("../../../images/personal-homepage/women.png") no-repeat;
  background-size: 100% 100%;
  position: relative;
  top: 3px;
  margin-left: 5px;
}
.main .main-box .content .m-info .u-model .u-modelName .model-name p {
  font-size: 12px;
}
.main .main-box .content .m-info .u-appraise {
  padding: 0 10px;
  border-bottom: 1px solid #E5E5E5;
  margin-bottom: 43px;
}
.main .main-box .content .m-info .u-appraise .u-title {
  line-height: 68px;
  font-size: 16px;
}
.main .main-box .content .m-info .u-appraise .appraise-box {
  margin-bottom: 40px;
}
.main .main-box .content .m-info .u-appraise .appraise-box .radio-box {
  float: left;
  padding-left: 60px;
  position: relative;
  margin-right: 155px;
  cursor: pointer;
}
.main .main-box .content .m-info .u-appraise .appraise-box .radio-box .checked {
  display: inline-block;
  width: 16px;
  height: 16px;
  border: 1px solid #A0A0A0;
  border-radius: 50%;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  margin: auto;
}
.main .main-box .content .m-info .u-appraise .appraise-box .good {
  background: url("../../../images/personal-homepage/order-management/hao.png") no-repeat;
  background-position: 30px -1px;
}
.main .main-box .content .m-info .u-appraise .appraise-box .ordinary {
  background: url("../../../images/personal-homepage/order-management/yiban.png") no-repeat;
  background-position: 30px -1px;
}
.main .main-box .content .m-info .u-appraise .appraise-box .dissatisfied {
  background: url("../../../images/personal-homepage/order-management/buhao.png") no-repeat;
  background-position: 30px -1px;
}
.main .main-box .content .m-info .u-appraise .appraise-box .active .checked {
  border-color: #E3A02B;
}
.main .main-box .content .m-info .u-appraise .appraise-box .active .circle {
  width: 80%;
  height: 80%;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  background: url(../../../images/personal-homepage/financial-management/circle.png) no-repeat;
  background-size: 100% 100%;
}
.main .main-box .content .m-info .u-appraise .message-title {
  margin-bottom: 15px;
  font-size: 16px;
}
.main .main-box .content .m-info .u-appraise .message-board {
  display: block;
  width: 633px;
  height: 170px;
  padding: 12px;
  box-sizing: border-box;
  border: 1px solid #ccc;
  margin-bottom: 47px;
}
.main .main-box .content .submit {
  width: 215px;
  height: 50px;
  color: #fff;
  text-align: center;
  line-height: 50px;
  background: #D6A003;
  margin: 0 auto;
  cursor: pointer;
  border-radius: 3px;
}
.main .main-box .content .submit:active {
  background: #B38608;
}
/*底部样式*/
.footer {
  background: #18292c;
}
.footer .content {
  padding: 36px 40px 0;
}
.footer .content .nav li {
  float: left;
  margin-right: 160px;
}
.footer .content .nav li p {
  font-size: 24px;
  line-height: 24px;
  color: #8c9496;
  margin-bottom: 18px;
}
.footer .content .nav li a {
  font-size: 16px;
  line-height: 30px;
  color: #48666a;
  padding: 5px 0;
  display: block;
}
.footer .content .code {
  border: 1px solid #465456;
  padding: 10px;
}
.footer .content .code img {
  margin-bottom: 8px;
}
.footer .content .code p {
  font-size: 14px;
  line-height: 14px;
  color: #48666a;
  text-align: center;
  margin-top: 4px;
}
.footer .copyright {
  padding: 40px 0;
  font-size: 12px;
  line-height: 20px;
  color: #10434f;
  text-align: center;
}
.fr {
  float: right!important;
}
.fl {
  float: left!important;
}
