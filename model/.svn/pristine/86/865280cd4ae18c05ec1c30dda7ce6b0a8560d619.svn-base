}
.main .main-box .annunciate-info .bd ul .end .annunciate-company .info-btn {
  width: 138px;
  height: 32px;
  line-height: 32px;
  text-align: center;
  color: #DDAF57;
  border: 2px solid #D2A93C;
  border-radius: 5px;
  cursor: pointer;
}
.main .main-box .annunciate-info .bd ul .end .annunciate-company .info-btn:active {
  border-color: #B38608;
  color: #B38608;
}
.main .main-box .annunciate-info .bd ul .end .annunciate-time {
  float: right;
  width: 110px;
  padding-top: 187px;
}
.main .main-box .annunciate-info .bd ul .end .annunciate-time .numbers {
  text-align: center;
  font-size: 12px;
  color: #A0A0A0;
}
.main .main-box .annunciate-info .bd ul .page-btn {
  width: 370px;
  margin: 0 auto;
  padding-top: 75px;
}
.main .main-box .annunciate-info .bd ul .page-btn i,
.main .main-box .annunciate-info .bd ul .page-btn span {
  font-size: 12px;
  border: 1px solid #CCC;
  line-height: 33px;
  height: 33px;
  display: block;
  float: left;
  margin-right: 12px;
  text-align: center;
  cursor: pointer;
}
.main .main-box .annunciate-info .bd ul .page-btn i {
  width: 53px;
}
.main .main-box .annunciate-info .bd ul .page-btn span {
  width: 33px;
}
.main .main-box .annunciate-info .bd ul .page-btn span.active {
  background: #000;
  border-color: #000;
  color: #fff;
}
/*底部样式*/
.footer {
  background: #18292c;
}
.footer .content {
  padding: 36px 40px 0;
}
.footer .content .nav li {
  float: left;
  margin-right: 160px;
}
.footer .content .nav li p {
  font-size: 24px;
  line-height: 24px;
  color: #8c9496;
  margin-bottom: 18px;
}
.footer .content .nav li a {
  font-size: 16px;
  line-height: 30px;
  color: #48666a;
  padding: 5px 0;
  display: block;
}
.footer .content .code {
  border: 1px solid #465456;
  padding: 10px;
}
.footer .content .code img {
  margin-bottom: 8px;
}
.footer .content .code p {
  font-size: 14px;
  line-height: 14px;
  color: #48666a;
  text-align: center;
  margin-top: 4px;
}
.footer .copyright {
  padding: 40px 0;
  font-size: 12px;
  line-height: 20px;
  color: #10434f;
  text-align: center;
}
.fr {
  float: right!important;
}
.fl {
  float: left!important;
}
