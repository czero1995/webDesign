body {
  /*font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;*/
  font-family: "微软雅黑", "Helvetica Neue", Helvetica, "PingFang SC", Arial, sans-serif;
  font-size: 14px;
  line-height: 26px;
  color: #333;
  background-color: #fff;
}
body,
html {
  min-width: 1200px;
  overflow-x: auto;
}
/*表单默认属性*/
input::-webkit-input-placeholder {
  color: #cccccc;
  opacity: 1;
}
textarea::-webkit-input-placeholder {
  color: #cccccc;
  opacity: 1;
}
input,
textarea {
  border-color: #CCCCCC;
  font-size: 14px;
  line-height: 26px;
  resize: none;
  outline: none;
  border: 1px solid #CCCCCC;
  text-indent: 10px;
}
textarea:focus {
  border-color: #D6A003;
}
input:focus {
  border-color: #D6A003;
}
.mg0 {
  margin: 0!important;
}
.mr40 {
  margin-right: 40px;
}
h3 {
  font-weight: normal;
}
.bdno {
  border: none!important;
}
.clearfloat:after {
  display: block;
  content: '';
  height: 0;
  width: 0;
  clear: both;
}
.container {
  width: 1200px;
  padding-right: 10px;
  padding-left: 10px;
  margin-right: auto;
  margin-left: auto;
  position: relative;
}

/*内容*/
.main {
  background: #1f1f1f;
}
.main .main-box {
  background: url("../../images/model-point/bg4.png") no-repeat;
  padding: 30px 0;
}
.main .main-box .banner {
  margin-bottom: 25px;
}
.main .main-box .banner img {
  width: 100%;
  display: block;
}
.main .main-box .annunciate-info .hd {
  background: #FAFAFA;
  width: 100%;
  border-bottom: 1px solid #E5E5E5;
  padding: 0 30px;
  box-sizing: border-box;
}
.main .main-box .annunciate-info .hd ul {
  float: left;
}
.main .main-box .annunciate-info .hd ul li {
  float: left;
  line-height: 65px;
  font-size: 16px;
  padding: 0 10px;
  border-bottom: 2px solid #FAFAFA;
  margin-right: 70px;
  cursor: pointer;
}
.main .main-box .annunciate-info .hd ul .on {
  color: #D9AE6C;
  border-color: #D49F40;
}
.main .main-box .annunciate-info .hd .sort {
  float: right;
}
.main .main-box .annunciate-info .hd .sort span {
  line-height: 67px;
  color: #A8A8A8;
  padding: 0 15px;
  cursor: pointer;
}
.main .main-box .annunciate-info .hd .sort .active {
  color: #000;
}
.main .main-box .annunciate-info .bd {
  background: #fff;
}
.main .main-box .annunciate-info .bd ul {
  padding: 0 30px;
  padding-bottom: 75px;
}
.main .main-box .annunciate-info .bd ul .going {
  padding: 30px 0;
  border-bottom: 1px solid #E5E5E5;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-img {
  float: left;
  width: 320px;
  height: 240px;
  margin-right: 30px;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-img img {
  display: block;
  width: 100%;
  height: 100%;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-company {
  float: left;
  width: 635px;
  padding-top: 10px;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-company .title {
  font-size: 18px;
  margin-bottom: 5px;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-company .title span {
  font-size: 16px;
  color: #DC5D60;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-company .title .redFlag {
  width: 14px;
  height: 19px;
  background: url("../../images/activity/flag-icon.png") no-repeat;
  background-size: 100% 100%;
  display: inline-block;
  margin-left: 15px;
  margin-right: 10px;
  position: relative;
  top: 3px;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-company .info {
  font-size: 14px;
  color: #A0A0A0;
  margin-bottom: 15px;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-company .address-info {
  margin-bottom: 25px;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-company .address-info .info-unit {
  width: 50%;
  float: left;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-company .info-btn {
  width: 138px;
  height: 32px;
  line-height: 32px;
  text-align: center;
  color: #DDAF57;
  border: 2px solid #D2A93C;
  border-radius: 5px;
  cursor: pointer;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-company .info-btn a{
  color: #DDAF57;}
.main .main-box .annunciate-info .bd ul .going .annunciate-company .info-btn:active {
  border-color: #B38608;
  color: #B38608;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-time {
  float: right;
  width: 110px;
  padding-top: 10px;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-time p {
  font-size: 12px;
  letter-spacing: 1px;
  margin-bottom: 5px;
  text-align: center;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-time .over-time {
  width: 84px;
  height: 46px;
  line-height: 46px;
  text-align: center;
  background: url("../../images/activity/over-time(2).png") no-repeat;
  background-size: 100% 100%;
  margin: 0 auto;
  margin-bottom: 100px;
  font-size: 22px;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-time .numbers {
  text-align: center;
  font-size: 12px;
  color: #A0A0A0;
}
.main .main-box .annunciate-info .bd ul .going .annunciate-time .numbers span {
  color: red;
}
.main .main-box .annunciate-info .bd ul .end {
  padding: 30px 0;
  border-bottom: 1px solid #E5E5E5;
}
.main .main-box .annunciate-info .bd ul .end .annunciate-img {
  float: left;
  width: 320px;
  height: 240px;
  margin-right: 30px;
}
.main .main-box .annunciate-info .bd ul .end .annunciate-img img {
  display: block;
  width: 100%;
  height: 100%;
}
.main .main-box .annunciate-info .bd ul .end .annunciate-company {
  float: left;
  width: 635px;
  padding-top: 10px;
}
.main .main-box .annunciate-info .bd ul .end .annunciate-company .title {
  font-size: 18px;
