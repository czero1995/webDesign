}
.main .main-box .annunciate-info .bd ul .end .annunciate-company .title span {
  font-size: 16px;
  color: #DC5D60;
}
.main .main-box .annunciate-info .bd ul .end .annunciate-company .title .redFlag {
  width: 14px;
  height: 19px;
  background: url("../../images/activity/flag-icon.png") no-repeat;
  background-size: 100% 100%;
  display: inline-block;
  margin-left: 15px;
  margin-right: 10px;
  position: relative;
  top: 3px;
}
.main .main-box .annunciate-info .bd ul .end .annunciate-company .info {
  font-size: 14px;
  color: #A0A0A0;
  margin-bottom: 15px;
}
.main .main-box .annunciate-info .bd ul .end .annunciate-company .address-info {
  margin-bottom: 25px;
}
.main .main-box .annunciate-info .bd ul .end .annunciate-company .address-info .info-unit {
  width: 50%;
