body {
  /*font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;*/
  font-family: "微软雅黑", "Helvetica Neue", Helvetica, "PingFang SC", Arial, sans-serif;
  font-size: 14px;
  line-height: 26px;
  color: #333;
  background-color: #fff;
}
body,
html {
  min-width: 1200px;
  overflow-x: auto;
}
/*表单默认属性*/
input::-webkit-input-placeholder {
  color: #cccccc;
  opacity: 1;
}
textarea::-webkit-input-placeholder {
  color: #cccccc;
  opacity: 1;
}
input,
textarea {
  border-color: #CCCCCC;
  font-size: 14px;
  line-height: 26px;
  resize: none;
  outline: none;
  border: 1px solid #CCCCCC;
  text-indent: 10px;
}
textarea:focus {
  border-color: #D6A003;
}
input:focus {
  border-color: #D6A003;
}
.mg0 {
  margin: 0!important;
}
h3 {
  font-weight: normal;
}
.bdno {
  border: none!important;
}
.clearfloat:after {
  display: block;
  content: '';
  height: 0;
  width: 0;
  clear: both;
}
.container {
  width: 1200px;
  padding-right: 0;
  padding-left: 0;
  margin-right: auto;
  margin-left: auto;
  position: relative;
}

/*内容*/
.main {
  background: #1f1f1f;
}
.main .main-box {
  background: url(../../images/model-point/bg4.png) center repeat-y;
  background-size: 100% 100%;
}
.main .main-box .slideTxtBox {
  width: 100%;
  text-align: left;
}
.main .main-box .slideTxtBox .hd {
  color: #989898;
  position: relative;
  margin-bottom: 65px;
  padding-top: 45px;
}
.main .main-box .slideTxtBox .hd ul li {
  float: left;
  cursor: pointer;
  text-align: center;
}
.main .main-box .slideTxtBox .hd ul li .nav-li {
  float: left;
  width: 128px;
}
.main .main-box .slideTxtBox .hd ul li .cutLine {
  float: left;
}
.main .main-box .slideTxtBox .hd ul .on .nav-li {
  color: #C8AA57;
}
.main .main-box .slideTxtBox .bd ul {
  zoom: 1;
  color: #C8AA57;
}
.main .main-box .slideTxtBox .bd  .bd-titleBox {
  position: relative;
  margin-bottom: 50px;
}
.main .main-box .slideTxtBox .bd  .bd-titleBox .bd-title {
  float: left;
  font-size: 30px;
  color: #C8AA57;
}
.main .main-box .slideTxtBox .bd  .bd-titleBox .hr {
  position: absolute;
  height: 1px;
  background: #C8AA57;
  top: 0;
  bottom: 0;
  left: 138px;
  margin: auto;
  width: 75%;
}
.main .main-box .slideTxtBox .bd  .bd-titleBox .bd-more {
  float: right;
  width: 138px;
  height: 28px;
  border: 2px solid #C8AA57;
  line-height: 30px;
  text-align: center;
  border-radius: 5px;
  cursor: pointer;
}
.main .main-box .slideTxtBox .bd  .bd-titleBox .bd-more a{
  width: 138px;
  height: 28px;
    color: #C8AA57;}
.main .main-box .slideTxtBox .bd  .bd-titleBox .bd-more:active {
  border: 2px solid #B38608;
  color: #B38608;
}
.main .main-box .slideTxtBox .bd  .photoBox  {
	position: relative;
  width: 100%;
}
.main .main-box .slideTxtBox .bd  .photoBox .photoWall:after,
.main .main-box .slideTxtBox .bd  .photoBox .photoWall:before {
content: " ";
display: table;
}
.main .main-box .slideTxtBox .bd  .photoBox .photoWall:after {
clear: both;
}
.main .main-box .slideTxtBox .bd  .photoBox ul{
	display: block;
	position: relative;
	width: 100%;
	padding-bottom: 25px;
	min-height: 420px;
}
.main .main-box .slideTxtBox .bd  .photoBox li {
  float: left;
  position: relative;
  overflow: hidden;
  width: 280px;
  padding: 10px;
}
.main .main-box .slideTxtBox .bd  .photoBox  li:hover img {
  transform: scale(1.1, 1.1);
  -ms-transform: scale(1.1, 1.1);
  -moz-transform: scale(1.1, 1.1);
  -webkit-transform: scale(1.1, 1.1);
  -o-transform: scale(1.1, 1.1);
}
.main .main-box .slideTxtBox .bd  .photoBox  li:hover .intro {
  transform: translate(0, 0);
  -webkit-transform: translate(0, 0);
  -ms-transform: translate(0, 0);
  -o-transform: translate(0, 0);
  -moz-transform: translate(0, 0);
}
.main .main-box .slideTxtBox .bd  .photoBox  img {
  display: block;
  width: 100%;
  transition: all .5s;
  -moz-transition: all .5s;
  -webkit-transition: all .5s;
  -o-transition: all .5s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
.main .main-box .slideTxtBox .bd  .photoBox  .img-title {
  background: url("../../images/model-point/bg2.png") no-repeat;
  background-size: 100% 100%;
  height: 85px;
  width: 100%;
  margin-bottom: 10px;
}
.main .main-box .slideTxtBox .bd  .photoBox  .img-title p {
  font-size: 24px;
  color: #9b7913;
  text-align: center;
  padding-top: 20px;
  margin-bottom: 5px;
  letter-spacing: 15px;
}
.main .main-box .slideTxtBox .bd  .photoBox  .img-title span {
  display: block;
  text-align: center;
  font-size: 12px;
  color: #b68904;
  position: relative;
  height: 25px;
}
.main .main-box .slideTxtBox .bd  .photoBox  .img-title span em {
  letter-spacing: 8px;
  z-index: 2;
  background: #FDF0D1;
  position: absolute;
  top: 50%;
  left: 50%;
  padding-left: 10px;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  text-align: center;
  float: left;
}
.main .main-box .slideTxtBox .bd  .photoBox  .img-title span .hr {
  height: 1px;
  background: #9b7913;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 65%;
  z-index: 1;
}
.main .main-box .slideTxtBox .bd  .photoBox  .intro {
  color: #fff;
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  text-align: center;
  background: rgba(0, 0, 0, 0.7);
  transform: translate(0, -100%);
  -webkit-transform: translate(0, -100%);
  -ms-transform: translate(0, -100%);
  -o-transform: translate(0, -100%);
  -moz-transform: translate(0, -100%);
  transition: all .5s;
  -moz-transition: all .5s;
  -webkit-transition: all .5s;
  -o-transition: all .5s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
  display: flex;
  align-items: center;
  justify-content: center;
}
.introa{
	
}
.main .main-box .slideTxtBox .bd  .photoBox  .intro h3 {
  display: inline-block;
  font-size: 18px;
  line-height: 18px;
  padding-bottom: 10px;
  margin-bottom: 10px;
  border-bottom: 1px solid #838383;
  font-weight: normal;
  text-align: center;
}
.main .main-box .slideTxtBox .bd  .photoBox  .intro p.address {
  padding: 0 8px 10px 28px;
  border-bottom: 1px solid #838383;
  background: url(../../images/ico_address.png) 10px top no-repeat;
  background-size: auto 14px;
  text-align: center;
}
.main .main-box .slideTxtBox .bd  .photoBox  .intro p {
  display: inline-block;
  font-size: 14px;
  line-height: 14px;
  margin-bottom: 10px;
  text-align: center;
}
.main .main-box .slideTxtBox .bd  .photoBox  .intro .items {
  display: inline-block;
  padding-top: 10px;
  border-top: 1px solid #838383;
  font-size: 0;
}
.main .main-box .slideTxtBox .bd  .photoBox  .intro .items span {
  display: inline-block;
  font-size: 12px;
  line-height: 18px;
  padding: 0 4px;
  background: #000;
  border-radius: 3px;
  margin: 0 3px;
}
.main .main-box .slideTxtBox .bd  .photoBox  .intro .grade {
  font-size: 14px;
}


/*底部样式*/
.footer {
  background: #18292c;
}
.footer .content {
  padding: 36px 40px 0;
}
.footer .content .nav li {
  float: left;
  margin-right: 160px;
}
.footer .content .nav li p {
  font-size: 24px;
  line-height: 24px;
  color: #8c9496;
  margin-bottom: 18px;
}
.footer .content .nav li a {
  font-size: 16px;
  line-height: 30px;
  color: #48666a;
  padding: 5px 0;
  display: block;
}
.footer .content .code {
  border: 1px solid #465456;
  padding: 10px;
}
.footer .content .code img {
  margin-bottom: 8px;
}
.footer .content .code p {
  font-size: 14px;
  line-height: 14px;
  color: #48666a;
  text-align: center;
  margin-top: 4px;
}
.footer .copyright {
  padding: 40px 0;
  font-size: 12px;
  line-height: 20px;
  color: #10434f;
  text-align: center;
}
.fr {
  float: right!important;
}
.fl {
  float: left!important;
}
