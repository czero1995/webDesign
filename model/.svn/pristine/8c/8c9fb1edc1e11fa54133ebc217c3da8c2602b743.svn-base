                      line-height: 30px;
                      width: 565px;
                      margin-bottom: 30px;
                      span{
                        display: block;
                        float: right;
                        width: 65px;
                        height: 28px;
                        line-height: 28px;
                        text-align: center;
                        border: 1px solid #D2A929;
                        color: #DBA02C;
                        border-radius:3px;
                        cursor: pointer;
                      }
                      span.silver{
                        border: 1px solid #ccc;
                        color:#ccc ;
                      }
                      em{
                        color: #A2A2A2;
                      }
                      em.black{
                        color: #000;
                      }
                      .wechat,.qq,.weibo{
                        display: inline-block;
                        position: relative;
                        top:5px;
                      }
                      .wechat{
                        width: 27px;
                        height: 22px;
                        background: url("../../../images/personal-homepage/basic-setup/wechat.png") no-repeat;
                        background-size: 100% 100%;
                      }
                      .qq{
                        width: 23px;
                        height: 27px;
                        background: url("../../../images/personal-homepage/basic-setup/QQ.png") no-repeat;
                        background-size: 100% 100%;
                      }
                      .weibo{
                        width: 29px;
                        height: 26px;
                        background: url("../../../images/personal-homepage/basic-setup/weibo.png") no-repeat;
                        background-size: 100% 100%;
                      }
                    }
                  }
                  .mod-3{
                    padding-top: 35px;
                    padding-left: 40px;
                    padding-bottom: 28px;
                    .u-title{
                      font-size:18px;
                      color: #DBA02C;
                      margin-bottom: 25px;
                    }
                    .card-box{
                      margin-bottom: 45px;
                      .bank-card{
                        float: left;
                        width: 258px;
                        height: 110px;
                        box-sizing: border-box;
                        border: 1px solid #E6E6E6;
                        border-radius:3px;
                        position: relative;
                        margin-right: 44px;
                        margin-bottom: 44px;
                        cursor: pointer;
                        .card-head{
                          height: 4px;
                          width: 100%;
                          margin-bottom: 20px;
                        }
                        .red{
                          background: #F40010;
                        }
                        .blue{
                          background: #1B6DB1;
                        }
                        .card-content{
                          padding-left: 19px;
                          .card-img{
                            float: left;
                            margin-right: 14px;
                            width: 30px;
                            height: 30px;
                            img{
                              width: 100%;
                              height: 100%;
                              display: block;
                            }
                          }
                          .card-info{
                            float: left;
                            font-size:14px;
                            color: #9D9D9D;
                            .card-name{
                              color: #000;
                              font-size:16px;
                              margin-bottom: 10px;
                            }
                          }
                        }
                        .f-card{
                          position: absolute;
                          top:0;
                          left: 0;
                          width: 100%;
                          height: 100%;
                          line-height: 108px;
                          text-align: center;
                          color: #fff;
                          background: rgba(0,0,0,.6);
                          opacity: 0;
                          filter: alpha(opacity=0);
                        }
                      }
                      .add-card{
                        float: left;
                        width: 256px;
                        height: 108px;
                        border: 1px dashed #E6E6E6;
                        position: relative;
                        span{
                          width: 105px;
                          height: 15px;
                          color: #DFA211;
                          text-align: right;
                          display: block;
                          position: absolute;
                          top:0;
                          bottom: 0;
                          left:0;
                          right: 0;
                          margin: auto;
                          cursor: pointer;
                          img{
                            position: absolute;
                            left:0;
                          }
                        }
                      }
                      .bank-card:hover{
                        .f-card{
                          opacity: 1;
                          filter: alpha(opacity= 100);
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}

