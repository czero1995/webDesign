body {
  /*font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;*/
  font-family: "微软雅黑", "Helvetica Neue", Helvetica, "PingFang SC", Arial, sans-serif;
  font-size: 14px;
  line-height: 26px;
  color: #333;
  background-color: #fff;
}
body,html{min-width: 1200px;overflow-x:auto;}

/*表单默认属性*/
input::-webkit-input-placeholder{
  color: #cccccc;opacity:1;
}
textarea::-webkit-input-placeholder{
  color: #cccccc;opacity:1;
}

input,textarea{
  border-color:#CCCCCC;
  font-size: 14px;
  line-height: 26px;
  resize: none;
  outline:none;
  border:1px solid #CCCCCC;
  text-indent: 10px;
}
textarea:focus{
  border-color:#D6A003;
}
input:focus{
  border-color:#D6A003;
}

.mg0{margin: 0!important;}
.mr40{margin-right: 40px;}
h3{font-weight: normal}
.bdno{
  border:none!important;}
.clearfloat:after{display: block;content: '';
  height: 0;
  width: 0;clear: both;}

.container {
  width: 1200px;
  padding-right: 10px;
  padding-left: 10px;
  margin-right: auto;
  margin-left: auto;
  position: relative;
}


/*头部*/
.header{
  background: #1f1f1f; padding: 18px 0; z-index: 9999;
  .logo{
    position: absolute; left:10px; top:50%; margin-top: -15px;
  }
  .nav{
    padding-left: 120px; padding-top: 16px;
    li{
      float:left;
      a{
        font-size: 16px; line-height: 28px; color:#989898; padding: 0 15px; margin:0 5px;
      }
    }
  }
  .divLink{
    position: absolute; right:10px; top:50%; margin-top:-22px;
    a{
      position: relative;
    }
    h3{
      font-size: 16px; line-height: 26px; color:#d9ab2b; font-weight: normal;
      span{
        display: inline-block; vertical-align: middle; width:10px; height: 6px; margin-left: 10px; background: url(../../../images/ico_down.png) center no-repeat;
      }
    }
    p{
      font-size: 12px; line-height: 18px; color:#999;
    }
    i{
      position: absolute; width:42px; height: 42px; border-radius: 50%; overflow: hidden; left:-50px; top:1px; background: url(../../../images/img_01.jpg) center no-repeat;
    }
  }
}
.header.active {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  background: rgba(31,31,31,0.9);
  /* transition: all 0.8s ease 0s; */
  opacity: 0;
  filter: alpha(opacity=0);
  -webkit-transform: translate(0, -80px);
  -moz-transform: translate(0, -80px);
  -ms-transform: translate(0, -80px);
  -o-transform: translate(0, -80px);
  transform: translate(0, -80px);
}
.header.selected {
  opacity: 1;
  filter: alpha(opacity=100);
  -webkit-transition: all 0.8s ease 0s;
  -moz-transition: all 0.8s ease 0s;
  -ms-transition: all 0.8s ease 0s;
  -o-transition: all 0.8s ease 0s;
  transition: all 0.8s ease 0s;
  -webkit-transform: translate(0, 0);
  -moz-transform: translate(0, 0);
  -ms-transform: translate(0, 0);
  -o-transform: translate(0, 0);
  transform: translate(0, 0px);
}


/*内容*/
.main{
  background: #fff;
  .main-box{
    background: url("../../../images/personal-homepage/bg.png") no-repeat;
    background-size: 100% auto;
    padding-top: 38px;
    padding-bottom: 25px;
    .content{
      border: 1px solid #E5E5E5;
      background: #fff;
      .m-title{
        line-height: 78px;
        padding-left: 28px;
        font-size:18px;
        border-bottom: 1px solid #E5E5E5;
      }
      .m-info{
        padding: 0 40px;
        .u-model{
          padding: 30px 10px;
          border-bottom: 1px solid #E5E5E5;
          .u-title{
            display: block;
            float: left;
            font-size: 16px;
          }
          .u-modelName{
            float: right;
            .head-img{
              float: left;
              width: 78px;
              height: 78px;
              margin-right: 20px;
              img{
                width: 100%;
                height: 100%;
                border-radius: 50%;
              }
            }
            .model-name{
              float: left;
              padding-top: 10px;
              p.name{
                font-size:14px;
                .sex{
                  width: 16px;
                  height: 19px;
                  display: inline-block;
                  background: url("../../../images/personal-homepage/women.png") no-repeat;
                  background-size: 100% 100%;
                  position: relative;
                  top:3px;
                  margin-left: 5px;
                }
              }
              p{
                font-size:12px;
              }
            }
          }
        }
        .u-money{
          padding: 0 10px;
          line-height: 69px;
          border-bottom: 1px solid #E5E5E5;
          .u-title{
            display: block;
            float: left;
            font-size: 16px;
          }
          .money{
            float: right;
            display: block;
            font-size:16px;
            color: #F8421D;
          }
        }
        .u-modeOfpayment{
          padding: 0 10px;
          margin-bottom: 34px;
          .u-title{
            padding: 28px 0;
            display: block;
            font-size:16px;
          }
          .modeOfpayment{
            .modeOfpayment-1{
              margin-bottom: 20px;
              .u-1-radio{
                float: left;
                height: 46px;
                line-height: 46px;
                position: relative;
                padding-left: 30px;
                margin-right: 85px;
                .checked{
                  display: inline-block;
                  width: 16px;
                  height: 16px;
                  border: 1px solid #A0A0A0;
                  border-radius:50%;
                  position: absolute;
                  top:0;
                  left:0;
                  bottom:0;
                  margin: auto;
                }
                .zhye{
                  color: #F8421D;
                }
                .zfb{
                  width: 119px;
                  height: 44px;
                  display: inline-block;
                  border: 1px solid #E5E5E5;
                }
                .wxzf{
                  width: 119px;
                  height: 44px;
                  display: inline-block;
                  border: 1px solid #E5E5E5;
                }
              }
              .active{
                .checked{
                  border-color:#E3A02B;
                  .circle{
                    width: 80%;
                    height: 80%;
                    position: absolute;
                    top:0;
                    bottom:0;
                    left:0;
                    right:0;
                    margin: auto;
                    background: url("../../../images/personal-homepage/financial-management/circle.png") no-repeat;
                    background-size: 100% 100%;
                  }
                }
              }
            }
            .modeOfpayment-2{
              margin-bottom: 20px;
              .u-2-radio-title{
                margin-bottom: 20px;
                position: relative;
                padding-left: 30px;
                .checked{
                  display: inline-block;
                  width: 16px;
                  height: 16px;
                  border: 1px solid #A0A0A0;
                  border-radius:50%;
                  position: absolute;
                  top:0;
                  left:0;
                  bottom:0;
                  margin: auto;
                }
                em{
                  color: #aaa;
                }
              }
              .u-2-radio{
                float: left;
                height: 46px;
                line-height: 46px;
                width: 155px;
                position: relative;
                padding-left: 38px;
                margin-right: 12px;
                margin-bottom: 12px;
                border: 1px solid #E5E5E5;
                box-sizing: border-box;
              }
              .checked{
                display: inline-block;
                width: 16px;
                height: 16px;
                border: 1px solid #A0A0A0;
                border-radius:50%;
                position: absolute;
                top:0;
                left:11px;
                bottom:0;
                margin: auto;
                cursor: pointer;
              }
              .active{
                .checked{
                  border-color:#E3A02B;
                  .circle{
                    width: 80%;
                    height: 80%;
                    position: absolute;
                    top:0;
                    bottom:0;
                    left:0;
                    right:0;
                    margin: auto;
                    background: url("../../../images/personal-homepage/financial-management/circle.png") no-repeat;
                    background-size: 100% 100%;
                  }
                }
              }
              .zfyh{
                display: block;
                position: absolute;
                top:0;
                left:35px;
                bottom:0;
                margin: auto;
              }
            }
            .radio{
              cursor: pointer;
            }
          }
        }
        .u-psw{
          padding-bottom: 50px;
          border-bottom: 1px solid #E5E5E5;
          margin-bottom: 95px;
          font-size:16px;
          span{
            line-height: 42px;
            display: block;
            float: left;
            margin-right: 15px;
          }
          .psw{
            width: 265px;
            height: 40px;
            border: 1px solid #E5E5E5;
            display: block;
            float: left;
            text-indent: 20px;
            color: #000;
          }
          .psw::placeholder{
            text-indent: 0;
            text-align: center;
            color: #D1D1D1;
          }
        }
        .u-submit{
          width: 215px;
          height: 50px;
          line-height: 50px;
          color: #fff;
          text-align: center;
          background: #D6A003;
          margin: 0 auto;
          margin-bottom: 20px;
          border-radius:3px;
          cursor: pointer;
        }
        .u-submit:active{
          background: #B38608;
        }
        .u-agreement{
          text-align: center;
          font-size:12px;
          padding-bottom: 80px;
          .agreement-btn{
            width: 215px;
            margin: 0 auto;
            cursor: pointer;
          }
          span{
            display: inline-block;
            height: 16px;
            width: 16px;
            border: 1px solid #9F9F9F;
            border-radius:50%;
            margin-right: 5px;
            position: relative;
            top:3px;
          }
          .active{
            border-color:#E3A02B;
            .circle{
                width: 80%;
                height: 80%;
                position: absolute;
                top:0;
                bottom:0;
                left:0;
                right:0;
                margin: auto;
                background: url("../../../images/personal-homepage/financial-management/circle.png") no-repeat;
                background-size: 100% 100%;
              }
          }
        }
      }
    }
  }
}

/*底部样式*/
.footer{
  background: #18292c;
  .content{
    padding:36px 40px 0;
    .nav{
      li{
        float:left; margin-right: 160px;
        p{
          font-size: 24px; line-height: 24px; color:#8c9496; margin-bottom: 18px;
        }
        a{
          font-size: 16px; line-height: 30px; color:#48666a; padding: 5px 0;display: block;
        }
      }
    }
    .code{
      border: 1px solid #465456; padding:10px;
      img{
        margin-bottom: 8px;
      }
      p{
        font-size: 14px; line-height: 14px; color:#48666a; text-align: center; margin-top: 4px;
      }
    }
  }
  .copyright{
    padding:40px 0; font-size: 12px; line-height: 20px; color:#10434f; text-align: center;
  }
}
.fr{
  float: right!important;}
.fl{
  float: left!important;
}