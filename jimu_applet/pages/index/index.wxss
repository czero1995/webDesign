/**index.wxss**/
.container{width: 100%;}
.slide-image{
  width: 100%;
}


.ProList{
  width: 100%;
  overflow: hidden;
  margin-top: 30rpx;
}
.ListBox{
  width: 305rpx;
  float: left;
  margin: 0 0 20rpx 20rpx;
  background: #fff;
  padding: 20rpx;
}
.ListBox image{
  width: 100%;
  height: 250rpx;
  display: block;
  margin-bottom: 20rpx;
}
.ListBox text{
  width: 100%;
  display: block;
}
.ListBox  .Title{
  font: normal 24rpx/30rpx "微软雅黑";
  margin-bottom: 20rpx;
}
.ListBox  .Pri{
  font: normal 22rpx/30rpx "微软雅黑";
}

.msg{
  width: 100%;
  text-align: center;
  font: normal 22rpx/30rpx "微软雅黑";
  padding: 20rpx 0 30rpx;
}