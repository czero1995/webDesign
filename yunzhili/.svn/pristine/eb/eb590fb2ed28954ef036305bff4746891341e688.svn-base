.select-model {
    position: absolute;
    top: 0%;
    left: 0%;
    //  display: none;
    background: rgba(0, 0, 0, 0.3);
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 9999;
    .select-content {
        position: absolute;
        left: 0;
        top: 0;
        width: 100%;
        background-color: #efefef;
        p {
            margin: 0;
            padding: 0;
        }
        .select-bar {
            margin: 0;
        }
        .select-box {
            margin-top: 0.2rem;
            padding: 0.3rem;
            background: #FFFFFF;
            .select-label {
                padding-top: 0.1rem;
                margin-right: 0.2rem;
                float: left;
                display: flex;
                flex-wrap: wrap;
                justify-content: space-between;
                .label-item {
                    padding: 0.05rem 0.1rem;
                    width: 100%;
                    margin-right:0.2rem;
                    margin-top: 0.2rem;
                    border-radius: 100px;
                    border: 1px solid #c9c9c9;
                    height: 0.4rem;
                    font-size: 0.24rem;
                    text-align: center;
                    line-height: 0.4rem;
                    color: #000000;
                    letter-spacing: 0.71px;
                }
                .active.label-item {
                    background: #000000;
                    color: #FFFFFF;
                }
            }
            .title-address {
                margin-top: 0.3rem;
            }
        }
        .select-address{
            background: #ffffff;
        }
    }
}

.phone-model {
