<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper" style="height: 1800px;">

{block name="header"}
    <header class="main-header">
        <!-- Logo -->
        <a href="#" class="logo"  style="background-color: #6bbb24">
            <!-- mini logo for sidebar mini 50x50 pixels -->
          <!--   <span class="logo-mini"><b>A</b>LT</span> -->
            <!-- logo for regular state and mobile devices -->
            <span class="logo-lg"  >云之力</span>
        </a>
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" style="background-color: #6bbb24">
            <!-- Sidebar toggle button-->
            <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                <span class="sr-only">Toggle navigation</span>
            </a>

            <div class="navbar-custom-menu">
                <ul class="nav navbar-nav">
                    <li class="dropdown user user-menu">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <img src="__STATIC__/admin/img/432670086831317036.png" class="user-image" >
                            <span class="hidden-xs">{$curUser}</span>
                        </a>
                        <ul class="dropdown-menu">
                            <!-- User image -->
                            <li class="user-header">
                                <img src="__STATIC__/admin/img/432670086831317036.png" class="img-circle" >

                                <p>
                                   {$curUser} - {$roleName}
                                </p>
                            </li>
                            <!-- Menu Body -->
                          
                            <!-- Menu Footer-->
                            <li class="user-footer">
                                
                                <div class="pull-right">
                                    <a href="{:url('admin/login/logout')}" class="btn btn-default btn-flat">退出登录</a>
                                </div>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
{/block}

{block name="left"}
    <aside class="main-sidebar">
        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">
            <!-- Sidebar user panel -->
            <div class="user-panel">
                <div class="pull-left image">
               
                    <img src="__STATIC__/admin/img/432670086831317036.png" class="img-circle" alt="User Image">
                </div>
                <div class="pull-left info">
                    <p>{$roleName}</p>
                    <a href="#"><i class="fa fa-circle text-success"></i> 在线</a>
                </div>
            </div>

            <!-- sidebar menu: : style can be found in sidebar.less -->
            <ul class="sidebar-menu" data-widget="tree">
                <li class="header">===========================</li>
                <li class="treeview {if condition='$active eq 8'} active {/if}">
                    <a href="#">
                        <i class="fa fa-area-chart"></i> <span>系统管理</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        <li class="{if condition='$active eq 8'} active {/if}">
                            <a href="{:URL('/admin/account/index')}"><i class="fa fa-circle-o"></i> 操作员管理</a>
                        </li>
                         <li class="{if condition='$active eq 8'} active {/if}">
                            <a href="{:URL('/admin/role/index')}"><i class="fa fa-circle-o"></i> 角色管理</a>
                        </li>
                    </ul>
                   
                </li>
                 <li class="treeview {if condition='$active eq 7'} active {/if}">
                    <a href="#">
                        <i class="fa fa-users"></i> <span>会员管理</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        <li class="{if condition='$active eq 7'} active {/if}">
                            <a href="{:URL('/admin/member/index')}"><i class="fa fa-circle-o"></i> 会员列表</a>
                        </li>
                    </ul>
                </li>
                <li class="treeview {if condition='$active eq 2'} active {/if}">
                    <a href="#">
                        <i class="fa fa-user"></i> <span>教练管理</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        <li class="{if condition='$active eq 2'} active {/if}">
                            <a href="{:URL('/admin/admin/coach')}"><i class="fa fa-circle-o"></i> 教练列表</a>
                        </li>
                        <li class="{if condition='$active eq 2'} active {/if}">
                            <a href="{:URL('/admin/admin/coachType')}"><i class="fa fa-circle-o"></i> 教练类型</a>
                        </li>
                    </ul>
                </li>

                <li class="treeview {if condition='$active eq 3'} active {/if}">
                    <a href="#">
                        <i class="fa fa-university"></i> <span>场馆管理</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        <li class="{if condition='$active eq 3'} active {/if}">
                            <a href="{:URL('/admin/stadium/stadium')}"><i class="fa fa-circle-o"></i> 场馆列表</a>
                        </li>
                        <li class="{if condition='$active eq 3'} active {/if}">
                            <a href="{:URL('/admin/stadium/stadiumType')}"><i class="fa fa-circle-o"></i> 场馆类型</a>
                        </li>
                    </ul>
                </li>
                <li class="treeview {if condition='$active eq 4'} active {/if}">
                    <a href="#">
                        <i class="fa fa-folder-open"></i> <span>课程管理</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        <li class="{if condition='$active eq 4'} active {/if}">
                            <a href="{:URL('/admin/course/index')}"><i class="fa fa-circle-o"></i> 课程列表</a>
                        </li>
                        <li class="">
                            <a href="{:URL('/admin/course/cate')}"><i class="fa fa-circle-o"></i> 课程类型</a>
                        </li>
                    </ul>
                </li>
                <li class="treeview {if condition='$active eq 6'} active {/if}">
                    <a href="#">
                        <i class="fa fa-tags"></i> <span>技能标签</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        <li class="{if condition='$active eq 6'} active {/if}">
                            <a href="{:URL('/admin/tags/index')}"><i class="fa fa-circle-o"></i> 标签列表</a>
                        </li>
                    </ul>
                </li>
                <li class="treeview {if condition='$active eq 5'} active {/if}">
                    <a href="#">
                        <i class="fa fa-flag"></i> <span>Banner管理</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu">
                        <li class="{if condition='$active eq 5'} active {/if}">
                            <a href="{:URL('/admin/Banner/index')}"><i class="fa fa-circle-o"></i> Banner列表</a>
                        </li>
                    </ul>
                </li>

        
            </ul>
        </section>
        <!-- /.sidebar -->
    </aside>
{/block}

