var paths = {
	'dev': 'http://api.jimuapi.com/wx/',
	'dt': 'http://wechat.fullstack.cn/',
	'product': 'http://api.szyunzhili.com/wx/'

}

path = paths.dev;

getOpenId()

function getOpenId() {
	var test = window.location.href;
	const that = this;
	axios.get(path + 'auth/getRedirectUrl?redirect=' + test, {

		}).then(function(res) {
			console.log('test',test);
			if(res.data.errcode == 200) {
				that.openurls = res.data.data;
				console.log('that.openurls', that.openurls);
				axios.get(path + 'getOpenid?redirect=' + that.urls, {

					}).then(function(res) {
						console.log('openid', res)
					})
					.catch(function(error) {
						console.log(error);
					});
			}

		})
		.catch(function(error) {
			console.log(error);
		});
}