.clearfloat::after{display: block;content: '';height: 0;width: 0;clear: both}
.framework7-root{
	overflow: auto;
}
p{
	margin: 0;
	padding: 0;
}
.swiper-container {
  height: 3.2rem!important;
    background: #ffffff;
}
.swiper-pagination {
  height: 0.02rem;
  position: absolute !important;
  top: 2.86rem !important;
}
.swiper-wrapper {
  /*height: 3.2rem;*/
}
.swiper-slide {
  background: #fff;
}
.swiper-slide span {
  text-align: center;
  display: block;
  margin: 20px;
  font-size: 21px;
}
.swiper-slide img{
	width: 100%;
	height: 100%;
}
.swiper-pagination-bullet {
  width: 0.5rem;
  height: 0.02rem;
}
.swiper-pagination-bullet-active {
  background: #FFFFFF!important;
}
.page {
  background: #EFEFEF;
}

.toolbar {
  height: 0.98rem;
}
.tabbar-label {
  font-family: PingFangSC-Regular;
  font-size: 0.18rem!important;
  color: #cccccc;
  letter-spacing: 0;
  padding-bottom: 0.14rem;
}
.tabbar a.link, .tabbar a.tab-link{
	width: 25%;
}
.title-name{
	  font-size: 0.28rem;
    color: #000000;
    letter-spacing: 0.82px;
    margin-bottom: 0.25rem;
    font-weight: 400;
}
.model{
	 	position: absolute;
    top: 0%;
    left: 0%;
    display: none;
    background: rgba(0, 0, 0, 0.3);
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 9999;
}

