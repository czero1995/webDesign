.framework7-root {
  overflow: auto;
}
body {
  background: #EFEFEF;
  overflow: auto;
}
body p {
  margin: 0;
  padding: 0;
}
.coachDetail-info {
  background: #FFFFFF;
  text-align: center;
}
.coachDetail-info .title-name {
  margin-bottom: 0.16rem;
}
.coachDetail-info .title-name .gender-img {
  position: relative;
  margin-left: 0.16rem;
  width: 0.2rem;
  height:0.2rem;
}
.coachDetail-info .coach-img {
  border-radius: 1rem;
  width: 1.1rem;
  height: 1.1rem;
  margin-bottom: 0.3rem;
  margin-top: 0.4rem;
}
.coachDetail-info .coach-rand {
  font-size: 0.26rem;
  color: #666666;
  letter-spacing: 0.76px;
  margin-bottom: 0.2rem;
}
.coachDetail-info .coach-label {
  display: flex;
  justify-content: center;
  margin-bottom: 0.2rem;
  /*margin-top: 0.32rem;*/
  display: inline-block;
}
.coachDetail-info .coach-label .label-item {
  margin-right: 0.2rem;
  padding-left: 0.2rem;
  padding-right: 0.2rem;
  border-radius: 100px;
  border: 1px solid #c9c9c9;
  height: 0.4rem;
  font-size: 0.24rem;
  text-align: center;
  line-height: 0.4rem;
  color: #000000;
  letter-spacing: 0.71px;
}
.coachDetail-info .coach-introduce {
  border-top: 1px solid #EFEFEF;
  padding: 0.3rem;
  font-family: PingFangSC-Regular;
  font-size: 0.26rem;
  color: #666666;
  height: 100%;
  letter-spacing: 0.76px;
  text-align: left;
  line-height: 0.44rem;
}
.coachDetail-tabBox {
  margin-top: 0.2rem;
}
.coachDetail-tabBox .tabBox-bar {
  height: 0.88rem;
  background: #FFFFFF;
  line-height: 0.88rem;
  border-bottom: 1px solid #CCCCCC;
}
.coachDetail-tabBox .tabBox-bar .active {
  border-bottom: 4px solid #000000;
  font-weight: bold;
}
.coachDetail-tabBox .tabBox-bar p {
  width: 50%;
  float: left;
  font-family: PingFangSC-Regular;
  font-size: 0.28rem;
  color: #000000;
  text-align: center;
  letter-spacing: 0.82px;
}
.coachDetail-tabBox .tabBox-lessonService .lessonService-item {
  margin-bottom: 0.2rem;
  background: #FFFFFF;
}
.coachDetail-tabBox .tabBox-lessonService .lessonService-item .lesson-box {
  padding: 0.3rem;
  border-bottom: 1px solid #EFEFEF;
  padding-bottom: 0.32rem;
}
.coachDetail-tabBox .tabBox-lessonService .lessonService-item .lesson-box .lesson-text {
  float: left;
  width: 58%;
}
.coachDetail-tabBox .tabBox-lessonService .lessonService-item .lesson-box .lesson-text .lesson-name {
  font-size: 0.28rem;
  color: #000000;
  letter-spacing: 0.82px;
  margin-bottom: 0.25rem;
}
.coachDetail-tabBox .tabBox-lessonService .lessonService-item .lesson-box .lesson-text .lesson-title {
  font-size: 0.24rem;
  color: #666666;
  letter-spacing: 0.71px;
  line-height: 0.4rem;
  height: 1.2rem;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}
.coachDetail-tabBox .tabBox-lessonService .lessonService-item .lesson-box .lesson-text .lesson-content {
  font-size: 0.24rem;
  color: #666666;
  letter-spacing: 0.71px;
}
.coachDetail-tabBox .tabBox-lessonService .lessonService-item .lesson-box .lesson-text .lesson-price {
  font-family: PingFangSC-Medium;
  font-size: 0.26rem;
  color: #FF0000;
  letter-spacing: 0.76px;
  margin-top: 0.3rem;
}
.coachDetail-tabBox .tabBox-lessonService .lessonService-item .lesson-box .lesson-img {
  float: right;
  width: 2.4rem;
  height: 2.4rem;
  border-radius: 4px;
}
.coachDetail-tabBox .tabBox-lessonService .lessonService-item .lesson-label {
  height: 0.8rem;
  margin-left: 0.3rem;
  display: flex;
}
.coachDetail-tabBox .tabBox-lessonService .lessonService-item .lesson-label .label-item {
  margin-right: 0.2rem;
  padding-left: 0.1rem;
  padding-right: 0.1rem;
  border-radius: 100px;
  border: 1px solid #C9C9C9;
  height: 0.4rem;
  line-height: 0.4rem;
  font-size: 0.24rem;
  text-align: center;
  color: #000000;
  letter-spacing: 0.71px;
  margin-top: 0.2rem;
}
.coachDetail-tabBox .tabBox-stadium .stadium-item {
  background: #FFFFFF;
  padding: 0.3rem;
  margin-bottom: 0.2rem;
}
.coachDetail-tabBox .tabBox-stadium .stadium-item .stadium-address img {
  vertical-align: middle;
  height: 0.3rem;
  width: 0.2rem;
  display: block;
  float: left;
}
.stadium-phone{
  display: inline-block;
  float: right;
  width: 0.6rem;
  height: 0.3rem;
}
.coachDetail-tabBox .tabBox-stadium .stadium-item .stadium-address span {
  font-family: PingFangSC-Regular;
  margin-left: 0.2rem;
  font-size: 0.24rem;
  color: #666666;
  letter-spacing: 1px;
  vertical-align: middle;
}
.coachDetail-tabBox .tabBox-stadium .stadium-item .stadium-address .phone-img {
  float: right;
  display: block;
  height: 0.24rem;
  width: 0.24rem;
}
