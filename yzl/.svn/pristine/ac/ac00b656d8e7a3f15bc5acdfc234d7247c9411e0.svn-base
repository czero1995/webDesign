  font-size: 0.26rem;
  line-height: 0.88rem;
  width: 100%;
}
.actions-modal.modal-in {
  width: 100%;
  margin: 0!important;
  left: 0;
}
.actions-modal.modal-in .actions-modal-group {
  width: 100%;
  margin: 0;
  border-radius: 0;
}
