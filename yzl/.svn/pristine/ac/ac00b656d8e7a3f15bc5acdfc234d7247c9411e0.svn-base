.list-block {
  margin: 0;
}
.list-block .item-info {
  margin-top: 0.2rem;
}
.list-block ul {
  background: #ebebeb;
}
.list-block ul li {
  background: #FFFFFF;
  height: 0.88rem;
  line-height: 0.88rem;
  font-family: PingFangSC-Regular;
  font-size: 0.26rem;
  color: #000000;
  margin: 0;
  letter-spacing: 0.76px;
  border-bottom: 1px solid #CCCCCC;
}
.list-block ul li .item-media + .item-inner {
  margin: 0;
}
.list-block ul li .item-content {
  margin: 0;
  padding: 0;
  padding: 0 0.3rem;
  height: 0.88rem;
}
.list-block ul li .item-content .username {
  text-align: right;
  font-weight: bold;
}
